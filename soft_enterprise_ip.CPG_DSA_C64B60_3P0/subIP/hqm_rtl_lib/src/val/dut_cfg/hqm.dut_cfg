ip_type=HQM

fuse_topology_settings::ip_type = HQM_FUSE
# Needed when overwrite fuses are identified
fuse_topology_settings::fuse_puller_type_name = hqm
fuse_topology_settings::fuse_ip_inst_name = 'NON_PMA'
fuse_topology_settings::fuse_ip_instance_name = dummy

# These RTL endpoints are used in hqm_arch.dut_cfg for making port connections.
gpsb_port {
    ip_type = GPSB_EP
    payload_width = 8
    connect_type = gpsb
    num_portids = 1
    portid_offset = 0
}
