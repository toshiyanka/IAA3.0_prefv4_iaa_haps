#This is to support scenario2 streaming direct test, running with illegal ldb qid (not programmed for ATM) from LDBPP1 

ldb pp 1 exp_ill_ldbqid=1
ldb pp 1 exp_drop=1

#PP[1] send illegal ENQ, if it sends out legit ENQ with QID=1, bypass the checking of ord
ldb qid 1 exp_remove_ord_q=255 exp_ord_not_empty=255 ord_mode=3
