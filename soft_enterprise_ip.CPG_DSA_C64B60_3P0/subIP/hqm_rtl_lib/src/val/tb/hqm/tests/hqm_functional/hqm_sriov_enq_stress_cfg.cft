   ldb pp 1 vas=7 
   ldb cq 1 gpa=sm qidv2=1 qidix2=4

   dir pp 62 vas=7
   dir cq 62 gpa=sm

   ldb pp 2 vas=21
   ldb cq 2 gpa=sm qidv5=1 qidix5=2

   dir pp 61 vas=21
   dir cq 61 gpa=sm

   ldb pp 3 vas=31 
   ldb cq 3 gpa=sm qidv7=1 qidix7=3

   dir pp 60 vas=31
