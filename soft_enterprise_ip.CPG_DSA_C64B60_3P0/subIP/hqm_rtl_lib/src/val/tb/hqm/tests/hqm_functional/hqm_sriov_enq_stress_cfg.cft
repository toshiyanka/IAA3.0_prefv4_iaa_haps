   vf 8  ldb_vpp30_pp=2 dir_vpp6_pp=61 ldb_vqid29_qid=2 dir_vqid2_qid=61
   vf 14 ldb_vpp31_pp=3 dir_vpp7_pp=60 ldb_vqid28_qid=3 dir_vqid3_qid=60

cfg_end

rd hqm_pf_cfg_i.vendor_id
