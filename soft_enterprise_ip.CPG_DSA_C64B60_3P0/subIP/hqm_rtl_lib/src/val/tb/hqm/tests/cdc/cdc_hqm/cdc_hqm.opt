
{
    %opts = (
        -model          => "cdc_hqm",
        -zin_top        => "cdc_hqm_rtl_lib.hqm",
        -zin_ctrl_files => [
            "$ENV{CDC_FLOW_RELEASE}/prototype/cdc_global.tcl",
            "$ENV{CDC_FLOW_RELEASE}/prototype/p1274/cdc_global_waivers.tcl",
            "$ENV{MODEL_ROOT}/tools/cdc/cdc_hqm.tcl",
        ],
        -cdc_post_process_modes       => "AceCDC_default,AceCDC_summary", 
        -enable_runsim_postsim_checks => 1,
        -create_postmortem_file       => 1,
        -postsim_log_max_num_warn     => 10,
        -post_process_parser          => "$ENV{ACE_HOME}/bin/ace_postsim.pl",
        -post_process_pattern_file    => "$ENV{ACE_HOME}/udf/ace_base_postsim.pp", ## any process pattern file in <dut>_hdl.udf file will override this
    );
    %runModes = (
        RptCLK => {
            -zin_run_type               => '-report_clock',
            -cdc_post_process_modes     => "AceCDC_default",
        },
        cfmRUN => {
            -zin_run_type            =>  '-hier',
            -cdc_post_process_modes => "AceCDC_default,AceCDC_summary",
        },
        hdmRUN => { 
            -zin_run_type            =>  '-hcdc',
            -cdc_post_process_modes => "AceCDC_default,AceCDC_summary,AceCDCLint_default",
        },  ##0p8 HDM enable
    );
}
