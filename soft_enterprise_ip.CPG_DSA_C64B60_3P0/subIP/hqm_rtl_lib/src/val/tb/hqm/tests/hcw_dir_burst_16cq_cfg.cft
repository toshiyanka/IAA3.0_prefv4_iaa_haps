  dir pp QID_15 gpa=0x800f0000 vas=0 dir_pool=DP_0 ldb_pool=LP_0 dir_credit_count=0x0  dir_min_credit_quanta=0x300 ldb_min_credit_quanta=0x100 dir_credit_hwm=0x500 dir_credit_lwm=0x200

  dir cq QID_0  gpa=0x80200000
  dir cq QID_1  gpa=0x80210000
  dir cq QID_2  gpa=0x80220000
  dir cq QID_3  gpa=0x80230000
  dir cq QID_4  gpa=0x80240000
  dir cq QID_5  gpa=0x80250000
  dir cq QID_6  gpa=0x80260000
