  dir pp QID_12 gpa=0x800c0000 vas=0 dir_pool=DP_0 ldb_pool=LP_0 dir_credit_count=0x0  dir_min_credit_quanta=0x300 ldb_min_credit_quanta=0x100 dir_credit_hwm=0x500 dir_credit_lwm=0x200
  dir pp QID_13 gpa=0x800d0000 vas=0 dir_pool=DP_0 ldb_pool=LP_0 dir_credit_count=0x0  dir_min_credit_quanta=0x300 ldb_min_credit_quanta=0x100 dir_credit_hwm=0x500 dir_credit_lwm=0x200
