rd hqm_system_csr.msix_ack              0x00000001
rd hqm_system_csr.msix_31_0_synd        0x00000001
rd hqm_system_csr.hqm_system_cnt_14     0x00000001
rd hqm_system_csr.hqm_system_cnt_16     0x00000001
#####################################################
# Cause another perr (this should not cause an MSI-X)
rd hqm_system_csr.ldb_qid_cfg_v[0]
# Check that syndrome more bit was set
rd hqm_system_csr.alarm_hw_synd         0xc0488400  # v=1 m=1 src=0 u=1 aid=0x09 cls=2 alarm=1 a=0x00
# Check that MSI-X was not sent
rd hqm_system_csr.hqm_system_cnt_14     0x00000001
rd hqm_system_csr.hqm_system_cnt_16     0x00000001
