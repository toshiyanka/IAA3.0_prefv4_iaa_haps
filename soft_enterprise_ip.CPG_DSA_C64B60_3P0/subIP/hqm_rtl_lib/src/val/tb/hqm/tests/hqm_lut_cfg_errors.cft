msix_alarm_wait 500
# Check syndrome
rd hqm_system_csr.alarm_hw_synd         0x80488400  # v=1 m=0 src=0 u=1 aid=0x09 cls=2 alarm=1 a=0x00
rd hqm_system_csr.ingress_lut_err       0x00000001
# Check that MSI-X was sent
rd hqm_system_csr.msix_ack              0x00000001
rd hqm_system_csr.msix_31_0_synd        0x00000001
rd hqm_system_csr.hqm_system_cnt_14     0x00000002
rd hqm_system_csr.hqm_system_cnt_16     0x00000002
# Clear syndrome
wr hqm_system_csr.alarm_hw_synd         0x80000000
rd hqm_system_csr.alarm_hw_synd         0x00488400
wr hqm_system_csr.ingress_lut_err       0x00000001
rd hqm_system_csr.ingress_lut_err       0x00000000
# Clear the msix_ack bit
wr hqm_system_csr.msix_ack              0x00000001
rd hqm_system_csr.msix_ack              0x00000000
wr hqm_system_csr.msix_31_0_synd        0x00000001
rd hqm_system_csr.msix_31_0_synd        0x00000000
#####################################################
# Check ingress_par_off (this should not cause a parity error or MSI-X)
wr hqm_system_csr.parity_ctl.ingress_par_off 1
rd hqm_system_csr.ldb_qid_cfg_v[0]
# Check syndrome was not set
rd hqm_system_csr.alarm_hw_synd         0x00488400
rd hqm_system_csr.ingress_lut_err       0x00000000
rd hqm_system_csr.msix_ack              0x00000000
rd hqm_system_csr.msix_31_0_synd        0x00000000
#####################################################
# This should cause another parity error and MSI-X
wr hqm_system_csr.parity_ctl.ingress_par_off 0
rd hqm_system_csr.ldb_qid_cfg_v[0]
msix_alarm_wait 500
# Check syndrome
rd hqm_system_csr.alarm_hw_synd         0x80488400  # v=1 m=0 src=0 u=1 aid=0x09 cls=2 alarm=1 a=0x00
rd hqm_system_csr.ingress_lut_err       0x00000001
# Check that MSI-X was sent
rd hqm_system_csr.msix_ack              0x00000001
rd hqm_system_csr.msix_31_0_synd        0x00000001
