rd hqm_system_csr.parity_ctl.write_bad_parity 1
wr hqm_system_csr.ecc_ctl.write_bad_mb_ecc    1
rd hqm_system_csr.ecc_ctl.write_bad_mb_ecc    1
# Write bad parity and ECC into LUTs
wr hqm_system_csr.vf_dir_vpp2pp[0]      0
wr hqm_system_csr.vf_ldb_vpp2pp[0]      0
wr hqm_system_csr.vf_dir_vpp_v[0]       0
wr hqm_system_csr.vf_ldb_vpp_v[0]       0
wr hqm_system_csr.dir_pp_v[0]           0
wr hqm_system_csr.ldb_pp_v[0]           0
wr hqm_system_csr.vf_dir_vqid2qid[0]    0
wr hqm_system_csr.vf_ldb_vqid2qid[0]    0
wr hqm_system_csr.vf_dir_vqid_v[0]      0
wr hqm_system_csr.vf_ldb_vqid_v[0]      0
wr hqm_system_csr.ldb_qid_its[0]        0
wr hqm_system_csr.dir_qid_its[0]        0
wr hqm_system_csr.dir_qid_v[0]          0
wr hqm_system_csr.ldb_qid_v[0]          0
wr hqm_system_csr.ldb_vasqid_v[0]       0
wr hqm_system_csr.dir_vasqid_v[0]       0
wr hqm_system_csr.ldb_pp2vas[0]         0
wr hqm_system_csr.dir_pp2vas[0]         0
wr hqm_system_csr.ldb_qid_cfg_v[0]      0
#
wr hqm_system_csr.ldb_qid2vqid[0]       0
wr hqm_system_csr.ldb_cq_pasid[0]       0
wr hqm_system_csr.ldb_cq_addr_l[0]      0
wr hqm_system_csr.ldb_cq_addr_u[0]      0
wr hqm_system_csr.ldb_cq2vf_pf_ro[0]    0
wr hqm_system_csr.dir_cq_pasid[0]       0
wr hqm_system_csr.dir_cq_addr_l[0]      0
wr hqm_system_csr.dir_cq_addr_u[0]      0
wr hqm_system_csr.dir_cq2vf_pf_ro[0]    0
wr hqm_system_csr.dir_cq_fmt[0]         0
#
wr hqm_msix_mem.vector_ctrl[9]          1
wr hqm_msix_mem.msg_data[9]             0
wr hqm_msix_mem.msg_addr_u[9]           0
wr hqm_msix_mem.msg_addr_l[9]           0
