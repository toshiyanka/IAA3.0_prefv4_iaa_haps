wr hqm_system_csr.ai_addr_u[0]          0
wr hqm_system_csr.ai_data[96]           0
wr hqm_system_csr.ai_addr_l[96]         0
wr hqm_system_csr.ai_addr_u[96]         0
wr hqm_system_csr.ldb_cq_isr[0]         0
wr hqm_system_csr.dir_cq_isr[0]         0
# Reset write_bad_parity and write_bad_ecc
wr hqm_system_csr.parity_ctl.write_bad_parity 0
rd hqm_system_csr.parity_ctl.write_bad_parity 0
