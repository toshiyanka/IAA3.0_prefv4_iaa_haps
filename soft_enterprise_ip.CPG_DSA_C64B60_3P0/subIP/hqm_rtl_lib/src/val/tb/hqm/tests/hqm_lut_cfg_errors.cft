rd hqm_system_csr.dir_qid_its[0]        0
rd hqm_system_csr.dir_qid_v[0]          0
rd hqm_system_csr.ldb_qid_v[0]          0
rd hqm_system_csr.ldb_vasqid_v[0]       0
rd hqm_system_csr.dir_vasqid_v[0]       0
rd hqm_system_csr.ldb_pp2vas[0]         0
rd hqm_system_csr.dir_pp2vas[0]         0
rd hqm_system_csr.ldb_qid_cfg_v[0]      0
#
rd hqm_system_csr.ldb_qid2vqid[0]       0
rd hqm_system_csr.ldb_cq_pasid[0]       0
rd hqm_system_csr.ldb_cq_addr_l[0]      0
rd hqm_system_csr.ldb_cq_addr_u[0]      0
rd hqm_system_csr.ldb_cq2vf_pf_ro[0]    0
rd hqm_system_csr.dir_cq_pasid[0]       0
rd hqm_system_csr.dir_cq_addr_l[0]      0
rd hqm_system_csr.dir_cq_addr_u[0]      0
rd hqm_system_csr.dir_cq2vf_pf_ro[0]    0
rd hqm_system_csr.dir_cq_fmt[0]         0
#
rd hqm_msix_mem.vector_ctrl[9]          1
rd hqm_msix_mem.msg_data[9]             0
rd hqm_msix_mem.msg_addr_u[9]           0
rd hqm_msix_mem.msg_addr_l[9]           0
rd hqm_system_csr.ai_data[0]            0
rd hqm_system_csr.ai_addr_l[0]          0
rd hqm_system_csr.ai_addr_u[0]          0
rd hqm_system_csr.ldb_cq_isr[0]         0
rd hqm_system_csr.dir_cq_isr[0]         0
#
rd hqm_system_csr.vf_dir_vpp2pp[9]      0
rd hqm_system_csr.vf_dir_vqid2qid[9]    0
rd hqm_system_csr.vf_ldb_vqid2qid[9]    0
#####################################################
# Should have been no errors
rd hqm_system_csr.alarm_hw_synd         0x00000000
rd hqm_system_csr.ingress_lut_err       0x00000000
rd hqm_system_csr.egress_lut_err        0x00000000
rd hqm_system_csr.alarm_lut_perr        0x00000000
