rd hqm_system_csr.ecc_ctl.write_bad_mb_ecc    0
#####################################################
# Set write_bad_sb_ecc
wr hqm_system_csr.ecc_ctl.write_bad_sb_ecc    1
rd hqm_system_csr.ecc_ctl.write_bad_sb_ecc    1
# Write bad ECC into LUTs
wr hqm_system_csr.vf_dir_vpp2pp[9]      0
wr hqm_system_csr.vf_dir_vqid2qid[9]    0
wr hqm_system_csr.vf_ldb_vqid2qid[9]    0
# Reset write_bad_sb_ecc
wr hqm_system_csr.ecc_ctl.write_bad_sb_ecc    0
rd hqm_system_csr.ecc_ctl.write_bad_sb_ecc    0
#####################################################
# Cause parity error by reading (this should cause an MSI-X)
rd hqm_system_csr.ldb_qid_cfg_v[0]
msix_alarm_wait 500
# Check that syndrome was set
rd hqm_system_csr.alarm_hw_synd         0x80488400  # v=1 m=0 src=0 u=1 aid=0x09 cls=2 alarm=1 a=0x00
rd hqm_system_csr.ingress_lut_err       0x00000001
