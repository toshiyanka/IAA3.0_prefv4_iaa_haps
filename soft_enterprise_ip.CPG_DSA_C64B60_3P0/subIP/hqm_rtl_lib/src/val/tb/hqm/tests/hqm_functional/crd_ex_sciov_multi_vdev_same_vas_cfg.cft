
cfg_begin

  dir qid DQ_A:*
  dir qid DQ_B:*
  ldb qid LQ_A:* fid_cfg_v=1
  ldb qid LQ_B:* sn_cfg_v=1  fid_cfg_v=1

  vas VS:* ldb_qidv:LQ_A=1 ldb_qidv:LQ_B=1 dir_qidv:DQ_A=1 dir_qidv:DQ_B=1 credit_cnt=8

  dir pp DQ_A    vas=VS 
  dir cq DQ_A    cq_depth=1024 gpa=sm

  dir pp DQ_B    vas=VS 
  dir cq DQ_B    cq_depth=1024 gpa=sm

  ldb pp LDB_PP_A:* vas=VS 
  ldb cq LDB_PP_A   gpa=sm qidv0=1 qidix0=LQ_A 

  ldb pp LDB_PP_B:* vas=VS
  ldb cq LDB_PP_B   gpa=sm qidv0=1 qidix0=LQ_B

  vdev VDEV_A:*  ldb_pp:LDB_PP_A:_v=1  ldb_vqid0_qid=LQ_A  dir_pp:DQ_A:_v=1 dir_vqid0_qid=DQ_A
  vdev VDEV_B:*  ldb_pp:LDB_PP_B:_v=1  ldb_vqid1_qid=LQ_B  dir_pp:DQ_B:_v=1 dir_vqid1_qid=DQ_B

  msix_alarm 0 enable=0x1 addr=0xaaaa5555aaaa5555 data=0xaaaa5555
  dir ims DQ_A enable=0x1 addr=0xaaaa9999aaaa9999 data=0xaaaa9999
  dir ims DQ_B enable=0x1 addr=0xaaaa6666aaaa6666 data=0xaaaa6666
  ldb ims LDB_PP_A enable=0x1 addr=0xaaaa7777aaaa7777 data=0xaaaa7777
  ldb ims LDB_PP_B enable=0x1 addr=0xaaaa8888aaaa8888 data=0xaaaa8888

cfg_end

wr hqm_system_csr.ingress_alarm_enable 0xffffffff
rd hqm_system_csr.ingress_alarm_enable 0x0000003f

