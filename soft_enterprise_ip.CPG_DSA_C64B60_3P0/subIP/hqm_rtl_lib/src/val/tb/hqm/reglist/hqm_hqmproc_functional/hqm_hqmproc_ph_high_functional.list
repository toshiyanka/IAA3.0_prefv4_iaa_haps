## Prochot High and Prochot tgl+hi

+defaults -ace_args -simv_args '"'+OVM_GLOBAL_TIMEOUT=10ms'"' -ace_args-

## -- To identify proc_hot regression mode
+defaults -ace_args -simv_args '"'+HQM_TB_PH_REGR'"' -ace_args- 

## -- increase the flr wait time for system burst tests
+defaults -ace_args -simv_args '"'+hqm_system_burst_user_data_seq_stim_config::flr_wait_time=8000'"' -ace_args-

## < run mode hqm_pwr_test:ph_high> 
+defaults -ace_args -simv_args '"'+HQM_PROCHOT_INIT_VAL=1'"' -ace_args-

.include hqm_hqmproc_functional.list 
-defaults -ace_args -simv_args '"'+HQM_PROCHOT_INIT_VAL=1'"' -ace_args-

## < run mode hqm_pwr_test:ph_tgl+ph_high> 
+defaults -ace_args -simv_args '"'+HQM_PH_MODE=ph_sequence +HQM_PH_DLY1_MIN=0 +HQM_PH_DLY1_MAX=20 +HQM_PH_DLY2_MIN=10 +HQM_PH_DLY2_MAX=50 +HQM_PH_SEQ_NUM=100 +HQM_PROCHOT_INIT_VAL=1'"' -ace_args-

.include hqm_hqmproc_functional.list 

-defaults -ace_args -simv_args '"'+HQM_PH_MODE=ph_sequence +HQM_PH_DLY1_MIN=0 +HQM_PH_DLY1_MAX=20 +HQM_PH_DLY2_MIN=10 +HQM_PH_DLY2_MAX=50 +HQM_PH_SEQ_NUM=100 +HQM_PROCHOT_INIT_VAL=1'"' -ace_args-

-defaults -ace_args -simv_args '"'+hqm_system_burst_user_data_seq_stim_config::flr_wait_time=8000'"' -ace_args-
-defaults -ace_args -simv_args '"'+OVM_GLOBAL_TIMEOUT=10ms'"' -ace_args-
-defaults -ace_args -simv_args '"'+HQM_TB_PH_REGR'"' -ace_args- 
