  simregress -notify -model hqm -dut hqm -num 1 -tmax 100 -P sc_normal3  -l verif/tb/hqm/reglist/hqm_compliance/hqm_iosf_test.list
  simregress -notify -model hqm -dut hqm -num 1 -tmax 100 -P sc_normal3  -l verif/tb/hqm/reglist/hqm_compliance/hqm_pcie_test.list
  simregress -notify -model hqm -dut hqm -num 1 -tmax 100 -P sc_normal3  -l verif/tb/hqm/reglist/reg_tests_uat.list
  simregress -notify -model hqm -dut hqm -num 1 -tmax 100 -P sc_normal3  -l verif/tb/hqm/reglist/reg_tests_uad.list
  simregress -notify -model hqm -dut hqm -num 1 -tmax 100 -P sc_normal3  -l verif/tb/hqm/reglist/hqm_functional/hqm_pwr_test.list -save -nofity

  simregress -notify -model hqm -dut hqm -num 1 -tmax 100 -P sc_normal3  -l verif/tb/hqm/reglist/hqm_functional/hqm_functional_agideq.list
