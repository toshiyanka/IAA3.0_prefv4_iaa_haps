#########################################################################################  11122020_HQMV25: Steve's email about controls in hqm_system
###################################################################################################
###################################################################################################
def disable_clock_gating (hqm):
   """Disables all clock gating in the HQM"""
   
   hqm.hqm_system_csr.prim_cdc_ctl.write(0x34444)
   hqm.hqm_system_csr.side_cdc_ctl.write(0x34444)
   hqm.hqm_system_csr.iosfp_cgctl.write(0x10)
   hqm.hqm_system_csr.iosfs_cgctl.write(0x10)
   hqm.hqm_cfg_master.cfg_hqm_cdc_control.write(0x34444)
   
   print((colorama.Fore.YELLOW+'HQM clock gating disabled'+colorama.Fore.RESET))
   
###################################################################################################
def enable_clock_gating (hqm):
   """Enables all clock gating in the HQM"""
   
   hqm.hqm_system_csr.prim_cdc_ctl.write(0x04444)
   hqm.hqm_system_csr.side_cdc_ctl.write(0x04444)
   hqm.hqm_system_csr.iosfp_cgctl.write(0x110)
   hqm.hqm_system_csr.iosfs_cgctl.write(0x110)
   hqm.hqm_cfg_master.cfg_hqm_cdc_control.write(0x04444)
   
   print((colorama.Fore.YELLOW+'HQM clock gating enabled'+colorama.Fore.RESET))
   
###################################################################################################
###################################################################################################
def enable_ingress_alarms (hqm):
   """Enables all ingress checks on enqueued HCWs in the HQM ingress logic"""
   
   hqm.hqm_system_csr.ingress_alarm_enable.write(0x3f)
   
   print((colorama.Fore.YELLOW+'HQM ingress alarms enabled'+colorama.Fore.RESET))

###################################################################################################
def disable_ingress_alarms (hqm):
   """Disables all ingress checks on enqueued HCWs in the HQM ingress logic"""

   hqm.hqm_system_csr.ingress_alarm_enable.write(0x00)
   
   print((colorama.Fore.YELLOW+'HQM ingress alarms disabled'+colorama.Fore.RESET))
   
###################################################################################################
###################################################################################################
def clear_aer (hqm):
   """Clears all HQM PCIe and AER error status regs. Note these regs are W1C."""
   
   hqm.hqm_pf_cfg_i.hqm_pcie_aer_cap_aer_cap_corr_err_status.write(0xffffffff)
   hqm.hqm_pf_cfg_i.hqm_pcie_aer_cap_aer_cap_uncorr_err_status.write(0xffffffff)
   hqm.hqm_pf_cfg_i.hqm_pcie_cap_pcie_cap_device_status.write(0x1f)
   hqm.hqm_pf_cfg_i.device_status.write(0xf900)
   
   print((colorama.Fore.YELLOW+'HQM PCIe and AER error status cleared'+colorama.Fore.RESET))
   
###################################################################################################
def unmask_aer (hqm):
   """Unmasks all HQM PCIe and AER errors and sets their severity to non-fatal"""
   
   hqm.hqm_pf_cfg_i.hqm_pcie_aer_cap_aer_cap_corr_err_mask.write(0x0)
   hqm.hqm_pf_cfg_i.hqm_pcie_aer_cap_aer_cap_uncorr_err_mask.write(0x0)
   hqm.hqm_pf_cfg_i.hqm_pcie_aer_cap_aer_cap_uncorr_err_sev.write(0x0)
   
   print((colorama.Fore.YELLOW+'HQM PCIe and AER errors unmasked and set to non-fatal'+colorama.Fore.RESET))
   
###################################################################################################
###################################################################################################
def clear_hw_alarms (hqm):
   """Clears the HQM alarm_hw_synd reg.  Note this reg is W1C."""
   
   hqm.hqm_system_csr.alarm_hw_synd.write(0xc0000000)
   
   print((colorama.Fore.YELLOW+'HQM HW alarm syndrome reg cleared'+colorama.Fore.RESET))
   
###################################################################################################
def clear_ingress_alarms (hqm):
   """Clears all HQM alarm_pf_synd and alarm_vf_synd0_* regs.  Note these regs are W1C."""
      
   hqm.hqm_system_csr.alarm_pf_synd_alarm_pf_synd0.write(0xc0000000)
   for vf in range(16):
     eval('hqm.hqm_system_csr.alarm_vf_synd_%s_alarm_vf_synd0'%(vf)).write(0xc0000000)
     
   print((colorama.Fore.YELLOW+'alarm syndrome regs cleared'+colorama.Fore.RESET))
   
###################################################################################################
def clear_all_alarms (hqm):
   """Clears all HQM alarm syndrome regs.  Note these regs are W1C."""

   clear_hw_alarms(hqm)
   clear_ingress_alarms(hqm)
   
###################################################################################################
###################################################################################################
def clear_all_errors (hqm):
   """Clears all HQM PCIe and AER error status regs, and alarm syndrome regs.  Note these regs are W1C."""

   clear_aer(hqm)
   clear_all_alarms(hqm)
   
###################################################################################################
def enable_all_errors (hqm):
   """Clears and enables all HQM errors (unmasks PCIe AER uncorrectable errors and sets their severity to non-fatal and enables all ingress checks)"""

   clear_all_errors(hqm)
   unmask_aer(hqm)
   enable_ingress_alarms(hqm)
      
###################################################################################################
###################################################################################################
def rate_limit_ingress_enq (hqm, rate_limit):
   """Sets the rate limiting for the ingress HCW pipeline.\nBits 0:8 of the rate_limit argument correspond to the respective ingress HCW pipeline level (p0-p8)\nand bit 9 corresponds to the DB at the bottom of the ingress HCW pipeline.\nSetting a bit means that the top of the pipeline cannot be loaded unless the pipeline\nlevel corresponding to that bit is not valid.  For example:\n\nSetting bit 0 means that HCWs will never enter the pipeline back-to-back (p0 must not be valid).\nSetting the value to 0x3ff ensures only one HCW can be in the entire pipeline at a time."""

   if ((rate_limit < 0) | (rate_limit > 0x3ff)):
      print('\n'+colorama.Fore.RED+'ERROR: rate_limit argument must be 0x000-0x3ff\n'+colorama.Fore.RESET)
   else:
      hqm.hqm_system_csr.ingress_ctl.enq_rate_limit.write(rate_limit)
      print((colorama.Fore.YELLOW+'HQM ingress HCW enqueue rate limiting set to 0x%x.'+colorama.Fore.RESET)%(rate_limit))
      
###################################################################################################
def rate_limit_egress_sch (hqm, rate_limit):
   """Sets the rate limiting for the egress HCW pipeline.\nBits 0:5 of the rate_limit argument correspond to the respective egress HCW pipeline level (sch_p0-sch_p5)\nand bit 6 corresponds to the DB at the bottom of the egress HCW pipeline.\nSetting a bit means that the top of the pipeline cannot be loaded unless the pipeline\nlevel corresponding to that bit is not valid.  For example:\n\nSetting bit 0 means that HCWs will never enter the pipeline back-to-back (sch_p0 must not be valid).\nSetting the value to 0x7f ensures only one HCW can be in the entire pipeline at a time."""

   if ((rate_limit < 0) | (rate_limit > 0x7f)):
      print('\n'+colorama.Fore.RED+'ERROR: rate_limit argument must be 0x00-0x7f\n'+colorama.Fore.RESET)
   else:
      hqm.hqm_system_csr.egress_ctl.sch_rate_limit.write(rate_limit)
      print((colorama.Fore.YELLOW+'HQM egress HCW schedule rate limiting set to 0x%x.'+colorama.Fore.RESET)%(rate_limit))
      
###################################################################################################
def rate_limit_wbuf_sch (hqm, rate_limit):
   """Sets the rate limiting for the write buffer pipeline.\nBits 0:2 of the rate_limit argument correspond to the respective write buffer pipeline level (p0-p2).\nSetting a bit means that the top of the pipeline cannot be loaded unless the pipeline\nlevel corresponding to that bit is not valid.  For example:\n\nSetting bit 0 means that HCWs will never enter the pipeline back-to-back (p0 must not be valid).\nSetting the value to 0x7 ensures only one HCW can be in the entire pipeline at a time."""

   if ((rate_limit < 0) | (rate_limit > 0x7)):
      print('\n'+colorama.Fore.RED+'ERROR: rate_limit argument must be 0x0-0x7\n'+colorama.Fore.RESET)
   else:
      hqm.hqm_system_csr.write_buffer_ctl.sch_rate_limit.write(rate_limit)
      print((colorama.Fore.YELLOW+'HQM write buffer rate limiting set to 0x%x.'+colorama.Fore.RESET)%(rate_limit))
         
###################################################################################################
def survival_wbuf_write_single_beats (hqm):
   """Sets the write buffer's write_single_beats chicken bit.\nThis will cause the write buffer to only write out single 16B HCWs with no pre or post padding to fill the 64B cache line."""
   
   hqm.hqm_system_csr.write_buffer_ctl.write_single_beats.write(1)
   
   print((colorama.Fore.YELLOW+'HQM write buffer set to write out only single HCWs w/o padding'+colorama.Fore.RESET))
          
###################################################################################################
def survival_wbuf_clear (hqm):
   """Clears any write buffer chicken bits that have been set.\nThis will cause the write buffer to return to it's normal cache line padding behavior\n(Rewriting earlier beats of the cache line before the current beat, if necessary, and padding out\n with 0s after the current beat if necessary, to fill the entire 64B cache line)."""

   hqm.hqm_system_csr.write_buffer_ctl.write_single_beats.write(0)
   
   print((colorama.Fore.YELLOW+'HQM write buffer set to normal operation (write full cache lines w/ padding)'+colorama.Fore.RESET))






