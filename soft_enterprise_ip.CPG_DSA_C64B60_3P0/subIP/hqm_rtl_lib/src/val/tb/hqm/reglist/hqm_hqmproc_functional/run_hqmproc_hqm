## -- PVIM 1407005866 ("Validate that Watch Dog Timers Only Affect prim_clkreq when a CQ times out.")
cq_dead_wd_timer:dir+pp0_tok_return_delay                 -dirtag dir_pp0_tok_return_delay

## -- PVIM 1604581701 ("Dead Thread Watchdog")
cq_dead_wd_timer:dir+pp0_tok_return_delay+wd_bg_cfg       -dirtag dir_pp0_tok_return_delay_wd_bg_cfg
cq_dead_wd_timer:dir+pp0_tok_return_delay+multiple_pp_en  -dirtag dir_pp0_tok_return_delay_multiple_pp_en
cq_dead_wd_timer:ldb+pp0_tok_return_delay                 -dirtag ldb_pp0_tok_return_delay
cq_dead_wd_timer:ldb+pp0_tok_return_delay+wd_bg_cfg       -dirtag ldb_pp0_tok_return_delay_wd_bg_cfg
cq_dead_wd_timer:ldb+pp0_tok_return_delay+multiple_pp_en  -dirtag ldb_pp0_tok_return_delay_multiple_pp_en
#
cq_dead_wd_timer:dir+ret_tok_on_msix                      -dirtag dir
cq_dead_wd_timer:ldb+ret_tok_on_msix                      -dirtag ldb
cq_dead_wd_timer:dir+multiple_pp_en                       -dirtag dir_multiple_pp_en
cq_dead_wd_timer:ldb+multiple_pp_en                       -dirtag ldb_multiple_pp_en
cq_dead_wd_timer:dir+wd_disable                           -dirtag dir_wd_disable
cq_dead_wd_timer:ldb+wd_disable                           -dirtag ldb_wd_disable

