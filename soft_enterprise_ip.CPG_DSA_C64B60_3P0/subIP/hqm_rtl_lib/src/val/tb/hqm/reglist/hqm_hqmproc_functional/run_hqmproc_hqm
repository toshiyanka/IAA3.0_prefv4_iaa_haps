hcw_pipeline_stress_test:si_20480 -ace -ace_args -simv_args +HQMS_DEBUG -simv_args +HQM_DEBUG_HIGH -ace_args- -ace_args -simv_args -ace_args- -ace_args -power -ace_args- -ace_args -simv_args -ace_args- -ace_args /nfs/site/disks/sdg74_2152/users/anyan/V23_gatekeeper/hqm-srvr10nm-wave4/tools/upf/hqm_power_warn -ace_args- -ace_args -simv_args -ace_args- -ace_args +HQMI_DEBUG -ace_args- -ace_args -simv_args -ace_args- -ace_args +HQMS_DEBUG -ace_args- -ace_args -simv_args -ace_args- -ace_args +OVM_VERBOSITY=OVM_LOW -ace_args- -ace_args -simv_args -ace_args- -ace_args -assert -ace_args- -ace_args -simv_args -ace_args- -ace_args finish_maxfail=100 -ace_args- -ace_args -simv_args -ace_args- -ace_args -assert -ace_args- -ace_args -simv_args -ace_args- -ace_args global_finish_maxfail=200 -ace_args- -ace_args -seed 1511c71 -ace_args- -assertFinal -chop scc -dbg_gdb_batch 1 -dut hqm -hqmi_debug -hqms_debug -model hqm -model hqm -no_flex_check -noexist -nosimix -ovm_low -seed 1511c71 -trexdefaults -ver /nfs/site/disks/sdg74_2152/users/anyan/V23_gatekeeper/hqm-srvr10nm-wave4 -yam_sn -ovm_low 

