idle 500

#################################################
wr hqm_system_csr.hqm_csr_cp_lo 0xFFFFFFFF
wr hqm_system_csr.hqm_csr_cp_hi 0xFFFFFFFF
wr hqm_system_csr.hqm_csr_rac_lo 0xFFFFFFFF
wr hqm_system_csr.hqm_csr_rac_hi 0xFFFFFFFF
wr hqm_system_csr.hqm_csr_wac_lo 0xFFFFFFFF
wr hqm_system_csr.hqm_csr_wac_hi 0xFFFFFFFF


#check device status and detected no error to be set
rd hqm_pf_cfg_i.device_status 0x0010

#check aer_cap_uncorr_err_status  and expect unsupporte request 
rd hqm_pf_cfg_i.aer_cap_uncorr_err_status 0x00100000

#check aer_corr err status non fatal
rd  hqm_pf_cfg_i.aer_cap_corr_err_status 0x00002000

#check ERror status
rd hqm_pf_cfg_i.pcie_cap_device_status 0x0009


#Check register  update happened or not
rd hqm_system_csr.cmpl_data_fifo_ctl 0x0000000C
rd hqm_system_csr.ti_sb_thresh 0x000000FE
rd hqm_system_csr.cmpl_hdr_fifo_ctl 0x00000006

#update check

rd hqm_pf_cfg_i.func_bar_l 0x0000000C
rd hqm_pf_cfg_i.func_bar_u 0x00000000

rd hqm_pf_cfg_i.csr_bar_l 0x0000000C
rd hqm_pf_cfg_i.csr_bar_u 0x00000002


rd hqm_pf_cfg_i.int_line 0x00

#wait for reset done
poll config_master.cfg_diagnostic_reset_status 0x000001ff 0x000081ff 1000




