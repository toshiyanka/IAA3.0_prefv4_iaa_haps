rd hqm_msix_mem.msg_data[3]    0x1ce1cebb
rd hqm_msix_mem.vector_ctrl[3] 0x00000000
###################################
# Enable MSI-X
wr hqm_pf_cfg_i.msix_cap_control 0x8000
rd hqm_pf_cfg_i.msix_cap_control 0x8047

