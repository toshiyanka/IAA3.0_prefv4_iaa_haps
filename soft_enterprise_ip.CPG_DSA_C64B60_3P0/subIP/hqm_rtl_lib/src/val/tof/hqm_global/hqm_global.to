-dut hqm
-model hqm
-noexist
-no_flex_check
-simv_args
  +CONFIGDB_CHECK_CONNECTIONS
  +CONFIGDB_DEBUG_MASK=7
  +DUT_CFG=simrun.dut_cfg
  +INTEGENV_DISABLE_PRINT
  +OVM_GLOBAL_TIMEOUT=2ms
  +OVM_VERBOSITY=OVM_MEDIUM
  +PMA_SB=ON
  +PROJCFG_DISABLE_SAD_CFG
  +RESET_AGENT_DUPLICATE_SEQ_OFF
  +SERVER_SIM_GPSB_RAL_ABORTED_MEM_RD_POST_1_1
  +SERVER_SIM_GPSB_RAL_ABORTED_MEM_WR_POST_1_1
  +SLA_CONFIG_PHASE_TIMEOUT=30000000
  +SLA_FLUSH_PHASE_TIMEOUT=400000
  +SLA_MAX_RUN_CLOCK=20000000
  +SLA_PCIE_FLUSH_PHASE_TIMEOUT=200000
  +SLA_USER_DATA_PHASE_TIMEOUT=20000000
  +hqm+HQM_PA_RANDOM_CORRUPT
  +iosf_pri_cfg_reg_rd_stim_config::dest_id=1
  +iosf_pri_cfg_reg_rd_stim_config::req_ch_id=0
  +iosf_pri_cfg_reg_rd_stim_config::src_id=0
  +iosf_pri_cfg_reg_wr_stim_config::dest_id=1
  +iosf_pri_cfg_reg_wr_stim_config::req_ch_id=0
  +iosf_pri_cfg_reg_wr_stim_config::src_id=0
  +iosf_pri_mem_reg_rd_stim_config::dest_id=1
  +iosf_pri_mem_reg_rd_stim_config::req_ch_id=0
  +iosf_pri_mem_reg_rd_stim_config::src_id=0
  +iosf_pri_mem_reg_wr_stim_config::dest_id=1
  +iosf_pri_mem_reg_wr_stim_config::req_ch_id=0
  +iosf_pri_mem_reg_wr_stim_config::src_id=0
  +iosf_sb_mem_reg_rd_stim_config::check_sai_illegal_reads=1
  +iosf_sb_mem_reg_wr_stim_config::check_sai_illegal_writes_status=1
  +iosf_tracker_filename=iosf_trk
  +lvm_dump_vpd_enable=1
  +lvm_dump_vpd_mem_enable=1
  +vcs+lic+wait
  +yamdo=$WORKAREA/cfg/yam_loads.do
  -assert finish_maxfail=1000000
  -assert global_finish_maxfail=1000000
  -assert hier=$WORKAREA/src/val/tb/hqm/tests/perm_hqm_master_pm.hier
  -assert nopostproc
  -power $WORKAREA/src/upf/hqm_power_warn
-simv_args-
