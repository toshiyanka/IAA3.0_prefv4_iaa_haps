-include $WORKAREA/src/val/tof/hqm_global/hqm_global.to
-simv_args
  +AW_CONTINUE_ON_ERROR
  +iosf_tracker_filename=iosf_trk
  -assert nopostproc
  +vcs+lic+wait
  +SLA_USER_DATA_PHASE_TIMEOUT=200000
  +SLA_CONFIG_PHASE_TIMEOUT=200000
  -assert finish_maxfail=1000000
  -assert global_finish_maxfail=1000000
  +hqm_pcie_init_stim_config::num_vfs=0
  +hqm_pcie_init_stim_config::skip_msix_cfg=1
  +hqm_pcie_init_stim_config::skip_msi_cfg=1
  +HQMS_DEBUG
  +HQMI_DEBUG
  +HQM_LSP_CQ_QID_CFG_CHECK_DIS
  +hqm_pok_flush_seq_stim_config::do_eot_rd_seq=1
-simv_args-
-ms -e_ppm hqm_test -ms-
