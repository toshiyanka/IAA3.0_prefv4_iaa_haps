   iNote "  tap2.dr_mux,         selection 3: IR.tir_out, unlock_red, unlock_orange_or_red = \$regB_OPCODE, \$regB_SECURITY -> dr_mux = regB.so"
   iWrite tap2.IR.IR[7:0] 0b01000000
   iApply

