   iNote "  tap2.regB.DR\[11:0\], load value = 101100111100"
   iWrite tap2.regB.DR[11:0] 0b101100111100
   iRead  tap2.regB.DR[11:0] 0bXXXXXXXX0101
   iApply -end_in_pause

