   iNote "  tap2.regB.DR\[11:0\], expected value = 101100111100"
   iWrite tap2.regB.DR[11:0] 0b000000000101
   iRead  tap2.regB.DR[11:0] 0b101100111100
   iApply

   iNote "Activate selection of the following scan muxes:"
   iNote "  tap2.tdo_mux,        selection 0: fsm.irsel = 1'b0 -> tdo_mux = dr_mux"
   iNote "  tap2.dr_mux,         selection 2: IR.tir_out, unlock_red, unlock_orange_or_red = \$regA2_OPCODE, \$regA2_SECURITY -> dr_mux = regA2.so"
   iWrite tap2.IR.IR[7:0] 0b00110001
