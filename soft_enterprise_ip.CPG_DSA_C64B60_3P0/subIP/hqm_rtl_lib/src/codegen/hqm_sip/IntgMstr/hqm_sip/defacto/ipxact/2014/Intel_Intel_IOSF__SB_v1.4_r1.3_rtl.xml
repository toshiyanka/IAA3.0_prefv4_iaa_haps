  <ipxact:description>Revision: r1.3. IOSF Sideband - Agent Interface.</ipxact:description>

  <ipxact:parameters>
    <ipxact:parameter parameterId="MESSAGEPAYLOADWIDTH" resolve="user" type="int">
      <ipxact:name>MESSAGEPAYLOADWIDTH</ipxact:name>
      <ipxact:description>IOSF sideband data payload size</ipxact:description>
      <ipxact:value>8</ipxact:value>
    </ipxact:parameter>
    <ipxact:parameter parameterId="SB_PARITY_REQUIRED" resolve="user" type="bit">
      <ipxact:name>SB_PARITY_REQUIRED</ipxact:name>
      <ipxact:description>Enable parity on IOSF sideband interface. When set to 1, enables parity support on the IOSF SBE Endpoint.</ipxact:description>
      <ipxact:value>0</ipxact:value>
    </ipxact:parameter>
    <ipxact:parameter parameterId="VECTOR_DIM0_SIZE" resolve="user" type="int">
      <ipxact:name>VECTOR_DIM0_SIZE</ipxact:name>
      <ipxact:description>Size of vector instance dimension 0</ipxact:description>
      <ipxact:value>1</ipxact:value>
    </ipxact:parameter>
    <ipxact:parameter parameterId="VECTOR_DIM0_LSB" resolve="user" type="int">
      <ipxact:name>VECTOR_DIM0_LSB</ipxact:name>
      <ipxact:description>LSB  of vector instance dimension 0</ipxact:description>
      <ipxact:value>0</ipxact:value>
    </ipxact:parameter>
    <ipxact:parameter parameterId="VECTOR_DIM1_SIZE" resolve="user" type="int">
      <ipxact:name>VECTOR_DIM1_SIZE</ipxact:name>
      <ipxact:description>Size of vector instance dimension 1</ipxact:description>
      <ipxact:value>1</ipxact:value>
    </ipxact:parameter>
    <ipxact:parameter parameterId="VECTOR_DIM1_LSB" resolve="user" type="int">
      <ipxact:name>VECTOR_DIM1_LSB</ipxact:name>
      <ipxact:description>LSB  of vector instance dimension 1</ipxact:description>
      <ipxact:value>0</ipxact:value>
    </ipxact:parameter>
    <ipxact:parameter parameterId="ARRAY_DIM0_SIZE" resolve="user" type="int">
      <ipxact:name>ARRAY_DIM0_SIZE</ipxact:name>
      <ipxact:description>Size of array instance dimension 0</ipxact:description>
      <ipxact:value>1</ipxact:value>
    </ipxact:parameter>
    <ipxact:parameter parameterId="ARRAY_DIM0_LSB" resolve="user" type="int">
      <ipxact:name>ARRAY_DIM0_LSB</ipxact:name>
      <ipxact:description>LSB  of array instance dimension 0</ipxact:description>
      <ipxact:value>0</ipxact:value>
    </ipxact:parameter>
    <ipxact:parameter parameterId="ARRAY_DIM1_SIZE" resolve="user" type="int">
      <ipxact:name>ARRAY_DIM1_SIZE</ipxact:name>
      <ipxact:description>Size of array instance dimension 1</ipxact:description>
      <ipxact:value>1</ipxact:value>
    </ipxact:parameter>
    <ipxact:parameter parameterId="ARRAY_DIM1_LSB" resolve="user" type="int">
      <ipxact:name>ARRAY_DIM1_LSB</ipxact:name>
      <ipxact:description>LSB  of array instance dimension 1</ipxact:description>
      <ipxact:value>0</ipxact:value>
    </ipxact:parameter>
    <ipxact:parameter parameterId="INST_SIZE" resolve="user" type="int">
      <ipxact:name>INST_SIZE</ipxact:name>
      <ipxact:description>Width multiplicative factor as function of other dimension sizes</ipxact:description>
      <ipxact:value>ARRAY_DIM0_SIZE*ARRAY_DIM1_SIZE*VECTOR_DIM0_SIZE*VECTOR_DIM1_SIZE</ipxact:value>
    </ipxact:parameter>
  </ipxact:parameters>

</ipxact:abstractionDefinition>
