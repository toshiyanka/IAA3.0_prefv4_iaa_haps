`include "hqm_dir_pipe.rdl"

addrmap hqm_dir_pipe_bridge {
    desc  = "hqm_dir_pipe bridge";
    bridge;

    //Instances of hqm PF top Registers.
    hqm_dir_pipe_map hqm_dir_pipe_MEM;
    hqm_dir_pipe_MEM->Space = "MEM";
    hqm_dir_pipe_MEM->BaseAddress = "TBD22";

    hqm_dir_pipe_map hqm_dir_pipe_MSG;
    hqm_dir_pipe_MSG->Space = "MSG";
    hqm_dir_pipe_MSG->Opcode = "MEM-SB";

};
