osetup nebulon
setenv PATH ${NEBULON_DIR}/bin:${PATH}

nebulon -input hqm_map.rdl -no_headers -ovm -I "$NEBULON_DATA/include . ../../subIP/sip/AW/src/rdl" -out_dir .

if ($status != 0) then
  echo "$0 Error: hqm_map.rdl nebulon command failed"
  exit
endif

nebulon -input hqm_core.rdl -no_headers -ovm -I "$NEBULON_DATA/include . ../../subIP/sip/AW/src/rdl" -out_dir .

if ($status != 0) then
  echo "$0 Error: hqm_core.rdl nebulon command failed"
  exit
endif
