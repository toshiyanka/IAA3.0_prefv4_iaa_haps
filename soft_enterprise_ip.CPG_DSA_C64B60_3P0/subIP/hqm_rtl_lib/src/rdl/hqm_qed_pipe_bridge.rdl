`include "hqm_qed_pipe.rdl"

addrmap hqm_qed_pipe_bridge {
    desc  = "hqm_qed_pipe bridge";
    bridge;

    //Instances of hqm PF top Registers.
    hqm_qed_pipe_map hqm_qed_pipe_MEM;
    hqm_qed_pipe_MEM->Space = "MEM";
    hqm_qed_pipe_MEM->BaseAddress = "TBD27";

    hqm_qed_pipe_map hqm_qed_pipe_MSG;
    hqm_qed_pipe_MSG->Space = "MSG";
    hqm_qed_pipe_MSG->Opcode = "MEM-SB";

};
