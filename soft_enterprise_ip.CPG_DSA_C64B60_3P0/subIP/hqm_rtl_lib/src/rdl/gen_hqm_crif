osetup nebulon
setenv PATH ${NEBULON_DIR}/bin:${PATH}

nebulon -input hqm_pf_cfg_top.rdl -crif -I "$NEBULON_DATA/include . ../../subIP/sip/AW/src/rdl" -out_dir .

if ($status != 0) then
  echo "$0 Error: hqm_pf_cfg_top.rdl nebulon command failed"
  exit
endif

nebulon -input hqm_csr_bar.rdl -crif -I "$NEBULON_DATA/include . ../../subIP/sip/AW/src/rdl" -out_dir .

if ($status != 0) then
  echo "$0 Error: hqm_csr_bar.rdl nebulon command failed"
  exit
endif

nebulon -input hqm_func_pf_bar.rdl -crif -I "$NEBULON_DATA/include . ../../subIP/sip/AW/src/rdl" -out_dir .

if ($status != 0) then
  echo "$0 Error: hqm_func_pf_bar.rdl nebulon command failed"
  exit
endif

