`include "hqm_atm_pipe.rdl"

addrmap hqm_atm_pipe_bridge {
    desc  = "hqm_atm_pipe bridge";
    bridge;

    //Instances of hqm PF top Registers.
    hqm_atm_pipe_map hqm_atm_pipe_MEM;
    hqm_atm_pipe_MEM->Space = "MEM";
    hqm_atm_pipe_MEM->BaseAddress = "TBD19";

    hqm_atm_pipe_map hqm_atm_pipe_MSG;
    hqm_atm_pipe_MSG->Space = "MSG";
    hqm_atm_pipe_MSG->Opcode = "MEM-SB";

};
