`include "hqm_system_csr.rdl"

addrmap hqm_system_csr_bridge {
    desc  = "hqm_system_csr bridge";
    bridge;
    USER_SPECIFIED_UDP = "HqmIsFeatureReg = reg";

    //Instances of hqm PF top Registers.
    hqm_system_csr_map hqm_system_csr_MEM;
    hqm_system_csr_MEM->Space = "MEM";
    hqm_system_csr_MEM->BaseAddress = "TBD3";

    hqm_system_csr_map hqm_system_csr_MSG;
    hqm_system_csr_MSG->Space = "MSG";
    hqm_system_csr_MSG->Opcode = "MEM-SB";

};
