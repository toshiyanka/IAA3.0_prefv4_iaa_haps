`include "hqm_nalb_pipe.rdl"

addrmap hqm_nalb_pipe_bridge {
    desc  = "hqm_nalb_pipe bridge";
    bridge;

    //Instances of hqm PF top Registers.
    hqm_nalb_pipe_map hqm_nalb_pipe_MEM;
