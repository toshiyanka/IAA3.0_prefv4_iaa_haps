<% use security; %>
<% my %Security_Props = security::GetSecurityInfo(); %>

  reg cfg_reset_vf_start_wo_r {
      Security_PolicyGroup = <%=$Security_Props{'HQM_OS_W'}%>;
      Security_ReadAccess_Str = <%=$Security_Props{'HQM_OS_W_RAC_AGENTS'}%>;
      Security_WriteAccess_Str = <%=$Security_Props{'HQM_OS_W_WAC_AGENTS'}%>;
      IntelRsvd=false;
      donttest=true;
      diag_reg=true;
      name = "Cfg Reset Vf Start";
      desc = "WRITE ONLY : INITIATES BROADCAST WRITE
[br]TYPE ENUM:
[br]([07:04]==3'd0 ) : LDB_CQ
[br]([07:04]==3'd1 ) : LDB_QID
[br]([07:04]==3'd2 ) : LDB_POOL
[br]([07:04]==3'd8 ) : DIR_CQ
[br]([07:04]==3'd9 ) : DIR_QID
[br]([07:04]==3'd10) : DIR_POOL
";
      regwidth = 32;
      shared;
      HandCoded=true;
      field { fieldwidth = 1; name = "Vf Reset Start"; desc = "BCAST: VF Reset Start"; AccessType = "WO"; PowerWell="vccsa"; IntelRsvd=false;} VF_RESET_START[0:0] = 1'h0;
      field { fieldwidth = 3; name = "Reserved.";      desc = "Reserved"; AccessType = "WO"; PowerWell="vccsa"; IntelRsvd=false;} RESERVED[3:1] = 3'h0;
      field { fieldwidth = 4; name = "Vf Reset Type";  desc = "VF Reset Type"; AccessType = "WO";  PowerWell="vccsa"; IntelRsvd=false;} VF_RESET_TYPE[07:04] = 4'h0;
      field { fieldwidth = 24; name = "Vf Reset Id";    desc = "VF Reset ID";  AccessType = "WO";  PowerWell="vccsa"; IntelRsvd=false;} VF_RESET_ID[31:08] = 24'h0;
  };
