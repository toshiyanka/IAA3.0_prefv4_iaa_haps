`include "hqm_sif_csr.rdl"

addrmap hqm_sif_csr_bridge {
    desc  = "hqm_sif_csr bridge";
    bridge;

    //Instances of hqm PF top Registers.
    hqm_sif_csr_map hqm_sif_csr_MEM;
    hqm_sif_csr_MEM->Space = "MEM";
    hqm_sif_csr_MEM->BaseAddress = "TBD3";

    hqm_sif_csr_map hqm_sif_csr_MSG;
    hqm_sif_csr_MSG->Space = "MSG";
    hqm_sif_csr_MSG->Opcode = "MEM-SB";

};
