#setenv EMUL_SYNPLIFY_VERSION I-2014.03_p1
#source /p/com/env/psetup/prod/bin/setupTool synplifypro $EMUL_SYNPLIFY_VERSION
## Point $EMUL_CFG_DIR to a local path if needed. Global release area is default ##
setenv EMUL_CFG_DIR /nfs/site/eda/group/SYSNAME/emu/intel/effm/latest/flows/emulation
