### Env vars for projects based on MODEL_ROOT
setenv   EMUL_TOOLS_DIR  $IP_ROOT/tools/emulation
setenv   EMU_CFG_DIR    $IP_ROOT/ace/emulation
setenv   EMUL_CFG_DIR    $IP_ROOT/ace/emulation
setenv   EMUL_RESULTS_DIR $IP_ROOT/pwa/results/emul

###############
## extract several variables from ACE settings.
$EMUL_TOOLS_DIR/ace_settings_info.pl $EMUL_CFG_DIR/ace_settings.tcsh
