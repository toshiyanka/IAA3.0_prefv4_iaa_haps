source $EMUL_CFG_DIR/ace_settings.tcsh


###############
###set path var
set path = ( ./ $EMUL_TOOLS_DIR $path /nfs/site/eda/group/SYSNAME/emu/intel/toolbox/current/bin/ )

##############sets up Synopsys Synplify Tools 
