	@echo   "    8. make LocalizeUPF -- To co-locate UPFs, update load_upf/source pointers, and update HIP versions"
	@echo   "    9. make InspectUPF -- To run syntax check on UPFs, pst_consistency check"
	@echo   "   10. make MergeUPF -- To merge multiple domains in UPFs, generating merged.upfs"
