
help :
	@echo ""
	@echo "*************************************** Help ***************************************"
	@echo ""
	@echo   "    1. make clean -- To clean the upf_utils output directory for provided PASS"
	@echo   "    2. make clean_LocalizeUPF -- To clean the upf_utils LocalizeUPF output directory for provided PASS"
	@echo   "    3. make clean_InspectUPF -- To clean the upf_utils InspectUPF output directory for provided PASS"
	@echo   "    4. make clean_MergeUPF -- To clean the upf_utils MergeUPF output directory for provided PASS"
