#For CNL
simbuild -dut dfxsecure_plugin -ace_args ace -cc -mc=dfxsecure_plugin_model -vlog_opts "+define+SVA_OFF" -en_xprop -ace_args- -nf -1c -CUST CNL -1c-
simbuild -dut dfxsecure_plugin -ace_args ace -x -t DfxSecurePlugin_DefaultTest -ace_args- -nf -1c -CUST CNL -1c-
simbuild -dut dfxsecure_plugin -ace_args ace -x -t DfxSecurePlugin_DriveUserPolicySeqTest -ace_args- -nf -1c -CUST CNL -1c-
simbuild -dut dfxsecure_plugin -ace_args ace -x -t DfxSecurePlugin_PolicySweepSeqTest -ace_args- -nf -1c -CUST CNL -1c-
simbuild -dut dfxsecure_plugin -ace_args ace -x -t DfxSecurePlugin_DriveUserPolicyViaSBOVRSeqTest -ace_args- -nf -1c -CUST CNL -1c-

\cp -r ../../target/dfxsecure_plugin/CNL/aceroot/pwa/results/vcs_lib/models/dfxsecure_plugin_model/unifiedInference.log . 
\cp -r ../../target/dfxsecure_plugin/CNL/aceroot/pwa/results/vcs_lib/models/dfxsecure_plugin_model/xprop.log .


