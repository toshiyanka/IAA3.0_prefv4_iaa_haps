\cp verif/tb/include/configs/2S/stap0_params_include.inc       verif/tb/include/configs/
\cp verif/tb/include/configs/2S/stap1_params_include.inc       verif/tb/include/configs/
\cp verif/tb/include/configs/2S/tapnw0_params_include.inc      verif/tb/include/configs/
\cp verif/tb/include/configs/2S/tb_param.inc                   verif/tb/include/

ace -cc -pwa pwa_2S
ace -x TapTestAllPrimary \
       TapTestAllSecondaryNormal -pwa pwa_2S -nb -nbmon

