\cp verif/tb/include/configs/default/tapnw0_params_include.inc verif/tb/include/configs/
\cp verif/tb/include/configs/default/tb_param.inc              verif/tb/include/

cd $IP_ROOT/
grep -ir status pwa/results/tests/*/postsim.log
