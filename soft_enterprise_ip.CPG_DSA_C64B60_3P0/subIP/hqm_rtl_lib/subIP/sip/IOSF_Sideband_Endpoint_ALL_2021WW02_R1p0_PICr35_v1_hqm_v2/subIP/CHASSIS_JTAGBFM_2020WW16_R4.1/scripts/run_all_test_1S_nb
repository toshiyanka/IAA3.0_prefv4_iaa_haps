cd $IP_ROOT/subIP/ip-tap-network/

\cp verif/tb/include/configs/1S/TapSeqLib.sv                   verif/tb/
\cp verif/tb/include/configs/1S/TapTop.sv                      verif/tb/
\cp verif/tb/include/configs/1S/mtap_tapnw_taps.sv             verif/tb/

\cp verif/tb/include/configs/1S/mtap0_params_include.inc       verif/tb/include/configs/
\cp verif/tb/include/configs/1S/stap0_params_include.inc       verif/tb/include/configs/
\cp verif/tb/include/configs/1S/tapnw0_params_include.inc      verif/tb/include/configs/
\cp verif/tb/include/configs/1S/tb_param.inc                   verif/tb/include/

ace -cc

ace -x TapTestAllPrimary:50:seed -nb -nbmon &

\cp verif/tb/include/configs/default/TapSeqLib.sv              verif/tb/
\cp verif/tb/include/configs/default/TapTop.sv                 verif/tb/
