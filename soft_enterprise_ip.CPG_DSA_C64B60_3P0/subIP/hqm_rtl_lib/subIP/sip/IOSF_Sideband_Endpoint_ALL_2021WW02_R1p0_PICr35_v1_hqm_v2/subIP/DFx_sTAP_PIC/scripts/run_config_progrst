\cp verif/tb/include/configs/STapTop_progrst_ext_lb.sv         verif/tb/STapTop.sv
\cp verif/tb/include/configs/emulate_stap_tdo_progrst.sv       verif/tb/emulate_stap_tdo.sv

ace -cc -pwa pwa_progrst
ace -x TapTestOnlyBypass \
       TapTestOnlySLVIDCODE \
       TapTestProgRstITDR \
       TapTestProgRstRTDR -pwa pwa_progrst -nb -nbmon

grep -ir status pwa/results/tests/*/postsim.log

