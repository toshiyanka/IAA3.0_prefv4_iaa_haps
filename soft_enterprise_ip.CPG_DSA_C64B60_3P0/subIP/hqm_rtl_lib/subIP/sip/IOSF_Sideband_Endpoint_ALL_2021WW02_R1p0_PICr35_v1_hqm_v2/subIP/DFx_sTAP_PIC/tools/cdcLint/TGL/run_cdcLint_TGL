   \cp tools/cdcLint/TGL/stap_waiver_w_TGL.xml tools/cdcLint/stap_waiver_w.xml
   setenv MODEL_ROOT $cwd
   setenv IP_ROOT $cwd
   setenv CDCLINT_HOME /p/hdk/rtl/proj_tools/cdclint/master/15.42.0/
   /p/hdk/rtl/proj_tools/cdclint/master/15.42.0/run_cdc_lint -res_dir tools/cdc/TGL/reports/reports_CFM/
   
   \rm tools/cdcLint/TGL/stap_violations.xml 
   \rm tools/cdcLint/TGL/stap_violations.out 

   \cp tools/cdcLint/stap_violations.xml tools/cdcLint/TGL/
   \cp tools/cdcLint/stap_violations.out tools/cdcLint/TGL/

   \rm  tools/cdcLint/stap_violations.xml
