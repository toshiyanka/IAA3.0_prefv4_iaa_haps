cd $IP_ROOT
mkdir -p $IP_ROOT/pwa/lint
echo "running lint in run_lint_config_default"
echo "Status for run_lint_config_default Follows:"                             > $IP_ROOT/pwa/lint/lint_log_for_all_configs
source $IP_ROOT/tools/lint/scripts/run_lint_config_default                    >> $IP_ROOT/pwa/lint/lint_log_for_all_configs
