#To run Emulation
echo "INFO: Running Emulation"
simbuild -dut stap -ace_args ace -cc -emul -ace_args- -1c -CUST CNL -1c-
echo "INFO: Copying the required emulation log files to tools/emulation folder."
\cp -rf target/stap/CNL/aceroot/pwa/results/emul/CHECK_*.log tools/emulation/
\cp -rf target/stap/CNL/aceroot/pwa/results/emul/emul_build_soc_status.log tools/emulation/

echo "INFO: Running Emulation"
simbuild -dut stap -ace_args ace -cc -emul -ace_args- -1c -CUST CDF -1c-
echo "INFO: Copying the required emulation log files to tools/emulation folder."
\cp -rf target/stap/CDF/aceroot/pwa/results/emul/CHECK_*.log tools/emulation/
\cp -rf target/stap/CDF/aceroot/pwa/results/emul/emul_build_soc_status.log tools/emulation/

