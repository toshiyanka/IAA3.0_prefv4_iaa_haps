cd $IP_ROOT
rm -rf pwa/results .acetestinfo.log

\cp source/rtl/stap_rtl.hdl_gls source/rtl/stap_rtl.hdl

\cp tools/syn/syn_1273/outputs/stap.syn_final.vg tools/syn/syn_1273/outputs/stap.syn_final.vg_bkp
\cp verif/tb/include/configs/stap.syn_final_tdo_posedge.vg tools/syn/syn_1273/outputs/stap.syn_final.vg 

\cp source/rtl/include/configs/stap_params_include_tdo_posedge.inc source/rtl/include/stap_params_include.inc
\cp verif/tb/include/configs/tb_param_default.inc              verif/tb/include/tb_param.inc
\cp verif/tb/include/configs/STapTop_tdo_posedge_gls.sv        verif/tb/STapTop.sv

ace -cc -vlog_opts "+define+no_unit_delay" -vlog_opts "+define+STAP_GLS=1" -pwa pwa_tdo_posedge_gls
ace -x TapTestOnlyBypass -vlog_opts "+define+no_unit_delay" \
       TapTestOnlySLVIDCODE -vlog_opts "+define+no_unit_delay" \
       TapTestBypass -vlog_opts "+define+no_unit_delay" \
       TapTestRegisterAccess -vlog_opts "+define+no_unit_delay" \
       TapTestFsmStates -vlog_opts "+define+no_unit_delay" \
       TapTestTmsGlitch -vlog_opts "+define+no_unit_delay" \
       TapTestNetworkRegisterAccess -vlog_opts "+define+no_unit_delay" \
       TapTestRandom -vlog_opts "+define+no_unit_delay" \
       TapTestBoundaryRegisterAccess -vlog_opts "+define+no_unit_delay" \
       TapTestReservedOpcodeAccess -vlog_opts "+define+no_unit_delay" \
       TapTestOrangeGreenRegisterAccess -vlog_opts "+define+no_unit_delay" \
       TapTestGreenRegisterAccess -vlog_opts "+define+no_unit_delay" -pwa pwa_tdo_posedge_gls -nb -nbmon

grep -ir status pwa/results/tests/*/postsim.log

\cp source/rtl/stap_rtl.hdl_rtl source/rtl/stap_rtl.hdl
`cp tools/syn/syn_1273/outputs/stap.syn_final.vg_bkp tools/syn/syn_1273/outputs/stap.syn_final.vg

