source /p/hdk/rtl/hdk.rc -cfg sip -reentrant
setenv MODEL_ROOT $cwd

simbuild -dut sbe -1c -CUST p1274 -toolset mat1.6.4.p2 -1c- -ace xterm &

to open hdk xterm 
simbuild -dut sbe -1c -CUST p1274 -toolset mat1.6.4.p2 -1c- -ver $cwd -ace xterm &

1. Compile the model
simbuild -dut sbe -1c -CUST p1274 -toolset mat1.6.4.p2 -1c- -ace_args ace -c -ace_args- -nf
simbuild -dut sbe -1c -CUST p1274 -toolset mat1.6.4.p2 -1c- -ace_args ace -c -x -t test01 -ace_args- -nf

2. 2-Stage FLG generation
febe -dut sbe -1c -CUST p1274 -toolset mat1.6.4.p2 -1c- -s all +s .tophiergen
febe -dut sbe -1c -CUST p1274 -toolset mat1.6.4.p2 -1c- -s all +s .v2k_prep
febe -dut sbe -1c -CUST p1274 -toolset mat1.6.4.p2 -1c- -s all +s .flg_v2k

3. Run ISAF (SpyglassDFT)
febe -dut sbe -1c -CUST s14nm -toolset mat1.6.4.p2 -1c- -s all +s spyglass_build
febe -dut sbe -1c -CUST s14nm -toolset mat1.6.4.p2 -1c- -s all +s sbendpoint.sgdft
   
4. Run DC
febe -dut sbe -1c -CUST p1274 -toolset mat1.6.4.p2 -1c- -s all +s gen_collateral +s build_blocksinfo +s sbendpoint.dc

5. Run FV
febe -dut sbe -1c -CUST p1274 -toolset mat1.6.4.p2 -1c- -s all +s sbendpoint.fv


