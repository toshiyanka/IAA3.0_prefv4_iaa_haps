PICr26: TSA 18.37.00, with mat1.5.3.p0 (contour N) and mat1.4L.11.p0 (contour N-1)

source /p/hdk/rtl/hdk.rc -cfg sip -reentrant
setenv MODEL_ROOT $cwd

s14nm
-----
wash -n intelall users soc socenv s14nm dk1273 soc73 soc73proc hdk10nm hdk10nmproc siphdk fabric hdk22nm hdk22nmproc
febe -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s lintra_build
febe -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s sbendpoint.lintra_elab
febe -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s gen_collateral +s build_blocksinfo +s sbendpoint.dc
febe -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s sbendpoint.fv
SGLP
   cp $MODEL_ROOT/tools/spyglasslp/sbendpoint/sbendpoint.opt $MODEL_ROOT/tools/spyglasslp/sbendpoint/sbendpoint.opt.p1274
   cp $MODEL_ROOT/tools/spyglasslp/sbendpoint/sbendpoint.opt.s14nm $MODEL_ROOT/tools/spyglasslp/sbendpoint/sbendpoint.opt
   febe -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s spyglass_build
   febe -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s spyglass_build_lp
   febe -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s sbendpoint.spyglass_lp
SGDFT
   febe -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s sbendpoint.sgdft
SAGE
   febe -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s sbendpoint.sage
VCLP
   febe -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s vclp_build
   febe -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s sbendpoint.vclp_test
      OR
   simbuild -dut sbe -1c -CUST p1274 -toolset m1713f.p3 -1c- -s all +s vclp_build -vclp_build "-ASSIGN -mc=cdc_sbendpoint"
   simbuild -dut sbe -1c -CUST p1274 -toolset m1713f.p3 -1c- -s all +s vclp_test -vclp_test "-t vclp/sbendpoint"

SGLINT
   simbuild -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s sglint_build -sglint_build "-ASSIGN -mc=sglint_sbendpoint"
   simbuild -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -s all +s sglint_test -sglint_test "-t spyglasslint/sbendpoint -noepi"
QUESTA CDC
  - run Questa CDC under ACE in HDK environment
      % simbuild -dut sbe -1c -CUST s14nm -toolset m1713f.p3 -1c- -ace xterm &
    In the newly created xterm:
      % cd $MODEL_ROOT/tools
      % sbendpoint_runACECDC_HDKs14nm
CALIBER
   CALIBER IS NOT A REQUIREMENT FOR THE ADP-S PROJECT (s14nm)
FISHTAIL
   FISHTAIL IS NOT A REQUIREMENT FOR THE ADP-S PROJECT (s14nm)
SGCDC 
   SGCDC IS NOT A REQUIREMENT FOR THE ADP-S PROJECT (s14nm)
INSERT_DFT
   INSERT_DFT IS NOT YET QUALIFIED FOR ADP-S, AT THE TIME OF THE SBE PICr29 RELEASE



MAT1.4.10plus.p2 (contour N) - p1274
------------------------------------
MAT version: ToolConfig.pl show_tool_info onecfg -1c -CUST p1274 -toolset mat1.5.3.p0 -1c-
simbuild -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -ace xterm &
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s lintra_build
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sbendpoint.lintra_elab
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s gen_collateral +s build_blocksinfo +s sbendpoint.dc
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sbendpoint.fv
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sbendpoint.sgdft
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sbendpoint.sage
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s spyglass_build
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s spyglass_build_lp
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sbendpoint.spyglass_lp
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s vclp_build
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sbendpoint.vclp_test
SGCDC
simbuild -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sgcdc_build -sgcdc_build "-ASSIGN -mc=sbe_config_ipxact_workspace -vlog_opts +define+functional+IOSF_SB_PH2"
simbuild -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sgcdc_test -sgcdc_test "-t spyglass_cdc/sbendpoint:SetupChk"
simbuild -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sgcdc_test -sgcdc_test "-t spyglass_cdc/sbendpoint"
SGLINT
simbuild -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sglint_build -sglint_build "-ASSIGN -mc=sglint_sbendpoint"
simbuild -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sglint_test -sglint_test "-t spyglasslint/sbendpoint -noepi"
CALIBER
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sbendpoint.caliber
FISHTAIL
febe -dut sbe -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -s all +s sbendpoint.fishtail
OPENLATCH
mv ace/p1274_e05_stdcells.hdl ace/p1274_e05_stdcells.hdl.KEEP
simbuild -dut sbe -ace_args ace -ccolt -mc=cdc_sbendpoint -ace_args- -1c -CUST p1274 -toolset mat1.5.3.p0 -1c- -nf
simbuild -dut sbe -ace_args ace -sc -t openlatch/sbendpoint -ace_args- -1c -CUST p1274 -toolset mat1.5.3.p0 -1c-
mv ace/p1274_e05_stdcells.hdl.KEEP ace/p1274_e05_stdcells.hdl
CORETOOLS version
ToolConfig.pl get_tool_version coretools -1c -CUST p1274 -toolset mat1.5.3.p0 -1c-
