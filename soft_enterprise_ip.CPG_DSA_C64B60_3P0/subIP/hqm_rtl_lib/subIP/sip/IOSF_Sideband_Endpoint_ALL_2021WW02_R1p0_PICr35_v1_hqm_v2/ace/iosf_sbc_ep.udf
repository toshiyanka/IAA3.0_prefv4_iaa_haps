#!/usr/intel/bin/perl
# for legacy non-hdk based runs
# null value for tsa_udf causes failures in ace. so just poulate the same ivars.udf twice
my $tsa_udf = $ENV{TSA_ROOT} ? "$ENV{TSA_ROOT}/cfg/ace/tsa_global_ivars.udf" : "ace/iosf_sbc_ep_local_ivars.udf";

%audf = (Includes => ["ace/iosf_sbc_ep_integ.udf",
                      "$tsa_udf",
                      "ace/iosf_sbc_ep_local_ivars.udf",
                      "ace/effm_flows/effmcheck.udf",]);
