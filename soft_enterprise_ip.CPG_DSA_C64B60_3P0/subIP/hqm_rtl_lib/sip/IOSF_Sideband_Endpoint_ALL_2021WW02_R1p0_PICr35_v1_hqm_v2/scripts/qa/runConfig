#set tools=(runATPG runCDC runLEC runLintra runSpyglassLP runACESpyglassDFT runSpyglassDFT runSynth runACECDC runACEEmulation runACEVisa)
#if (${dynopt} != "nonDyn" || ${config} == "IOSF_Sideband_Endpoint" ) set tools=(${tools} runVisa)
#set tools_febe=(runSynth)

set cmdline=""
set cmdline="${cmdline}-config \'${config}\' "
set cmdline="${cmdline}-component \'${component}\' "
set cmdline="${cmdline}-puni_prefix \'${puniprefix}\' "
set cmdline="${cmdline}-dyn_opt \'${dynopt}\' "

# Legacy Non HDK tool runs, not needed for now
#foreach tool (${tools})
#   set script_name=${TOOLS_DIR}/${component}_${tool}
#   echo "#\!/bin/tcsh -f"                  >  ${script_name}
#   echo "setenv SIMBUILDTOOLDRUN 0"        >> ${script_name}
#   echo "cd ../scripts/qa/"                >> ${script_name}
#   echo "source ${tool} ${cmdline}"'$argv' >> ${script_name}
#   echo "unsetenv SIMBUILDTOOLDRUN"        >> ${script_name}
#   chmod +x                                   ${script_name}

#   if ( $debug ) echo "${SIP_TOOL_NAME} -D- Created helper script ${script_name}"
#
#end

#foreach tool (${tools_febe})
#   set script_name=${TOOLS_DIR}/${component}_${tool}HDK
#   echo "#\!/bin/tcsh -f"                  >  ${script_name}
#   echo "setenv SIMBUILDTOOLDRUN 0"        >> ${script_name}
#   echo "cd ../scripts/qa/"                >> ${script_name}
#   echo "source ${tool} ${cmdline}"'$argv' >> ${script_name}
#   echo "unsetenv SIMBUILDTOOLDRUN"        >> ${script_name}
#   chmod +x                                   ${script_name}
#
#   if ( $debug ) echo "${SIP_TOOL_NAME} -D- Created helper script ${script_name}"
#
#end


set tools_simbuild=(runLintra runACECDC runSpyglassLP runVisa runACEEmulation runACEVisa)
set cust_simbuild=(p1273 p1274 s14nm n7 n6)
if (${config} == "IOSF_Sideband_Endpoint" ) then
   set hdk_dut = sbe
else
   set hdk_dut = sbr
endif

foreach cust (${cust_simbuild})
   if (${cust} == "s14nm" ) then
       set hdk_mat = mat1.6.4.p1
   else
        if (${cust} == "n7" || ${cust} == "n6") then
         set hdk_mat = mat1.6.1.p7
      else
         set hdk_mat = mat1.6.4.p2
      endif
   endif
         foreach tool (${tools_simbuild})
      set script_name=${TOOLS_DIR}/${component}_${tool}_HDK${cust}
      echo "#\!/bin/tcsh -f"                  >  ${script_name}
      echo "simbuild -dut ${hdk_dut} -1c -CUST ${cust} -toolset ${hdk_mat} -1c- -ace_args scripts/qa/${tool} ${cmdline}"'$argv -ace_args-' >> ${script_name}
      chmod +x                                   ${script_name}

      if ( $debug ) echo "${SIP_TOOL_NAME} -D- Created helper script ${script_name}"
   end
end

# for simbuild/febe tools the scripts in scripts/qa dir has the simbuild commands in them. So it is enough to just source those
set tools_sg=(runSGCDC runVCLP runSGLint)
foreach cust (${cust_simbuild})
   foreach tool (${tools_sg})
      set script_name=${TOOLS_DIR}/${component}_${tool}_HDK${cust}
      echo "#\!/bin/tcsh -f"                  >  ${script_name}
      echo "simbuild -dut ${hdk_dut} -1c -CUST ${cust} -1c- -ace_args scripts/qa/${tool} ${cmdline}"'$argv -ace_args-' >> ${script_name}
      chmod +x                                   ${script_name}

      if ( $debug ) echo "${SIP_TOOL_NAME} -D- Created helper script ${script_name}"
   end
end

if ( $debug ) echo "${SIP_TOOL_NAME} -D- Completed for the configuration ${component}"

cd ${SBR_DIR}
