#!/bin/tcsh -f
# name: runConfig

# TODO: Any manipulation of data not through parseXml should no longer be
#       required. Change as soon as possible.

################################################################################
# Source a script that is called at the start of each back-end 
# scripts/qa/run* script
################################################################################
source ./runPre -tool_name $0 $argv
if( $status != 0 ) exit $status

# Does this need to be done anymore?
if (! $?SIP_CONFIG_FILE) then
  setenv SIP_CONFIG_FILE sbr_config_ipxact_${config}.xml
endif

if ("${config}" == "IOSF_Sideband_Endpoint") then
   echo "${SIP_TOOL_NAME} -I- Endpoint(${component}) with PUNI_PREFI(${puniprefix})"
   pushd ../../
   setenv SBE_DIR $cwd
   popd
   set TOOLS_DIR=${SBE_DIR}/tools
else
   set cmdline=""
   set cmdline="${cmdline}-ip_name '${ipname}' "
   set cmdline="${cmdline}-config '${config}' "
   set cmdline="${cmdline}-component '${component}' "
   set cmdline="${cmdline}-puni_prefix '${puniprefix}' "
   set cmdline="${cmdline}-dyn_opt '${dynopt}' "
   set cmdline="${cmdline}-generate_collateral "
   set cmdline="${cmdline}-no_run "

   echo "${SIP_TOOL_NAME} -I- Router(${component}) with DYN_OPT(${dynopt}) and PUNI_PREFIX(${puniprefix})"

   set TOOLS_DIR=${SBR_DIR}/tools
   set CONST_PARSEXML=${SBR_DIR}/unsupported/export/data/const
   set PRE_PARSEXML=${SBR_DIR}/unsupported/export/data/xml
   set POST_PARSEXML=${SBR_DIR}/unsupported/export
   if ( ${dynopt} == "nonDyn" ) then
      set RTLTOP=${component}
      set PUNITOP=${component}
   else
      set RTLTOP=${component}_sbr_generic
      set PUNITOP=${puniprefix}${component}
   endif


   ######################
   # Creating LINT area #
   ######################
   
   #set LINTRA_DIR=${TOOLS_DIR}/lintra/${component}
   set LINTRA_DIR=${TOOLS_DIR}/lintra/${RTLTOP}
   mkdir -p ${LINTRA_DIR}
   source ${SBR_DIR}/scripts/qa/runLintra ${cmdline}
   if ( $debug ) echo "${SIP_TOOL_NAME} -D- Lintra colateral generated at ${LINTRA_DIR}"
   

   #############################
   # Creating SpyglassDFT area #
   #############################
   set SPYGLASSDFT_DIR=${TOOLS_DIR}/spyglassdft/router/${component}
   mkdir -p ${SPYGLASSDFT_DIR}
   # ACE SpyglassDFT setup
   source ${SBR_DIR}/scripts/qa/runACESpyglassDFT ${cmdline}
   # Legacy SpyglassDFT setup
   mv ${POST_PARSEXML}/${component}_spyglass_dft.prj               ${SPYGLASSDFT_DIR}/..
   mv ${POST_PARSEXML}/${component}_scan_ready.sgdc                ${SPYGLASSDFT_DIR}/..


   #####################
   # Creating CDC area #
   #####################
   set CDC_DIR=${TOOLS_DIR}/cdc/tests/${RTLTOP}
   
   mkdir -p                                                        ${CDC_DIR}
#Jesse: Adding in ACE CDC TCL Flow - START
   source ${SBR_DIR}/scripts/qa/runACECDC ${cmdline}
#Jesse: Adding in ACE CDC TCL Flow - FINISH

   cp ${POST_PARSEXML}/${component}_cdc_ctrl.v                     ${CDC_DIR}/iosf_sbc_rtr_${RTLTOP}_cdc_ctrl.v
   mkdir -p                                                        ${CDC_DIR}/tcl_ctrl_files
   cp ${PRE_PARSEXML}/debug_flags.v.tcl                            ${CDC_DIR}/tcl_ctrl_files
   cp ${POST_PARSEXML}/${component}_cdc.list                       ${CDC_DIR}/rtl.list
   if ( $debug ) echo "${SIP_TOOL_NAME} -D- CDC colateral generated at ${CDC_DIR}"
   
   #####################
   # Creating SYN area #
   #####################
   set SYNTH_DIR=${TOOLS_DIR}/syn
   ######## 1271 Synthesis ########
   set SYNTH1271_DIR=${SYNTH_DIR}/syn_1271/${component}/rdt/scripts
   
   mkdir -p ${SYNTH1271_DIR}

   echo 'cd ../../../../'                                                             >  ${SYNTH1271_DIR}/../${component}_run_syn
   echo 'source $KIT_PATH/kit_setup'                                                  >> ${SYNTH1271_DIR}/../${component}_run_syn
   echo "${component}"'_runSynth $argv'                                               >> ${SYNTH1271_DIR}/../${component}_run_syn
   chmod +x                                                                              ${SYNTH1271_DIR}/../${component}_run_syn
   
   # Generate the Scripts to execute a 1271 specific synthesis run
   set COPY_FROM_DIR=${POST_PARSEXML}
   cp ${COPY_FROM_DIR}/${component}_rtl_list.tcl                                         ${SYNTH1271_DIR}/rtl_list.tcl
   /usr/intel/bin/perl -pi -e 's/<SIPINT>/SIPINT_1271/g'                                 ${SYNTH1271_DIR}/rtl_list.tcl
   cp ${COPY_FROM_DIR}/${component}_block_setup.tcl                                      ${SYNTH1271_DIR}/block_setup.tcl
   
   # Const for 1271
   set COPY_FROM_DIR=${CONST_PARSEXML}/syn/scripts_1271/
   cp ${COPY_FROM_DIR}/global_constraints.tcl                                            ${SYNTH1271_DIR}/global_constraints.tcl
   cp ${COPY_FROM_DIR}/router_clock_gate.tcl                                             ${SYNTH1271_DIR}/${RTLTOP}_clock_gate.tcl
   
   # Variable Files for 1271
   set COPY_FROM_DIR=${POST_PARSEXML}
   cp  ${COPY_FROM_DIR}/${component}_additional_upf_setup.tcl                            ${SYNTH1271_DIR}/${RTLTOP}_additional_upf_setup.tcl
   cp  ${COPY_FROM_DIR}/${component}_io_constraints.tcl                                  ${SYNTH1271_DIR}/${RTLTOP}_io_constraints.tcl
   cp  ${COPY_FROM_DIR}/${component}_clocks.stcl                                         ${SYNTH1271_DIR}/${RTLTOP}_clocks.tcl
   cp  ${COPY_FROM_DIR}/${component}_scan_config_1273.tcl                                ${SYNTH1271_DIR}/${RTLTOP}_scan_config.tcl
   cat ${COPY_FROM_DIR}/${component}_scon.stcl ${COPY_FROM_DIR}/${component}_tcon.stcl > ${SYNTH1271_DIR}/global_cross_clk_exceptions.tcl
   
   if ( $debug ) echo "${SIP_TOOL_NAME} -D- SYN colateral generated at ${SYNTH1271_DIR}"
   
   ######## 1273 Synthesis ########
   set SYNTH1273_DIR=${SYNTH_DIR}/syn_1273/${component}/rdt/scripts
   
   mkdir -p ${SYNTH1273_DIR}
   
   echo 'cd ../../../../'                                                             >  ${SYNTH1273_DIR}/../${component}_run_syn
   echo 'source $KIT_PATH/kit_setup'                                                  >> ${SYNTH1273_DIR}/../${component}_run_syn
   echo "${component}"'_runSynth $argv'                                               >> ${SYNTH1273_DIR}/../${component}_run_syn
   echo "popd"                                                                        >> ${SYNTH1273_DIR}/../${component}_run_syn
   chmod +x                                                                              ${SYNTH1273_DIR}/../${component}_run_syn

   # Generate the Scripts to execute a 1273 specific synthesis run
   set COPY_FROM_DIR=${POST_PARSEXML}
   cp ${COPY_FROM_DIR}/${component}_rtl_list.tcl                                         ${SYNTH1273_DIR}/rtl_list.tcl
   /usr/intel/bin/perl -pi -e 's/<SIPINT>/SIPINT_1273/g'                                 ${SYNTH1273_DIR}/rtl_list.tcl
   cp ${COPY_FROM_DIR}/${component}_block_setup.tcl                                      ${SYNTH1273_DIR}/block_setup.tcl
   
   # Variable Files for 1273
   set COPY_FROM_DIR=${POST_PARSEXML}
   cp  ${COPY_FROM_DIR}/${component}_additional_upf_setup.tcl                            ${SYNTH1273_DIR}/${RTLTOP}_additional_upf_setup.tcl
   cp  ${COPY_FROM_DIR}/${component}_io_constraints.tcl                                  ${SYNTH1273_DIR}/${RTLTOP}_io_constraints.tcl
   cp  ${COPY_FROM_DIR}/${component}_clocks.stcl                                         ${SYNTH1273_DIR}/${RTLTOP}_clocks.tcl
   cp  ${COPY_FROM_DIR}/${component}_scan_config_1273.tcl                                ${SYNTH1273_DIR}/${RTLTOP}_scan_config.tcl
   cat ${COPY_FROM_DIR}/${component}_scon.stcl ${COPY_FROM_DIR}/${component}_tcon.stcl > ${SYNTH1273_DIR}/global_cross_clk_exceptions.tcl
   
   if ( $debug ) echo "${SIP_TOOL_NAME} -D- SYN colateral generated at ${SYNTH1273_DIR}"
   
   ######## 10nm Synthesis ########
   set SYNTH1274_DIR=${SYNTH_DIR}/syn_1274/${component}/rdt/scripts
   
   mkdir -p ${SYNTH1274_DIR}
   
   echo 'cd ../../../../'                                                             >  ${SYNTH1274_DIR}/../${component}_run_syn
   echo 'source $KIT_PATH/kit_setup'                                                  >> ${SYNTH1274_DIR}/../${component}_run_syn
   echo "${component}"'_runSynth $argv'                                               >> ${SYNTH1274_DIR}/../${component}_run_syn
   echo "popd"                                                                        >> ${SYNTH1274_DIR}/../${component}_run_syn
   chmod +x                                                                              ${SYNTH1274_DIR}/../${component}_run_syn

   # Generate the Scripts to execute a 10nm specific synthesis run
   set COPY_FROM_DIR=${POST_PARSEXML}
   cp ${COPY_FROM_DIR}/${component}_rtl_list.tcl                                         ${SYNTH1274_DIR}/rtl_list.tcl
   /usr/intel/bin/perl -pi -e 's/<SIPINT>/SIPINT_1274/g'                                 ${SYNTH1274_DIR}/rtl_list.tcl
   cp ${COPY_FROM_DIR}/${component}_block_setup.tcl                                      ${SYNTH1274_DIR}/block_setup.tcl
   cp ${COPY_FROM_DIR}/${component}_insert_dft.tcl                                       ${SYNTH1274_DIR}/insert_dft.tcl
   
   # Variable Files for 10nm
   set COPY_FROM_DIR=${POST_PARSEXML}
   cp  ${COPY_FROM_DIR}/${component}_additional_upf_setup.tcl                            ${SYNTH1274_DIR}/${RTLTOP}_additional_upf_setup.tcl
   cp  ${COPY_FROM_DIR}/${component}_io_constraints.tcl                                  ${SYNTH1274_DIR}/${RTLTOP}_io_constraints.tcl
   cp  ${COPY_FROM_DIR}/${component}_clocks.stcl                                         ${SYNTH1274_DIR}/${RTLTOP}_clocks.tcl
   cp  ${COPY_FROM_DIR}/${component}_scan_config_1274.tcl                                ${SYNTH1274_DIR}/${RTLTOP}_scan_config.tcl
   cat ${COPY_FROM_DIR}/${component}_scon.stcl ${COPY_FROM_DIR}/${component}_tcon.stcl > ${SYNTH1274_DIR}/global_cross_clk_exceptions.tcl
   
   if ( $debug ) echo "${SIP_TOOL_NAME} -D- SYN colateral generated at ${SYNTH1274_DIR}"


   ######## PUNI checker mode ########
   cp -r ${CONST_PARSEXML}/uniquification ${SBR_DIR}/tools

   ######## FEBE config ########
   #cp ${CONST_PARSEXML}/febe/LocalToolData.pm ${SBR_DIR}/cfg/LocalToolData.pm
   cp ${POST_PARSEXML}/${component}_LocalToolData.pm ${SBR_DIR}/cfg/LocalToolData.pm
   cp ${CONST_PARSEXML}/febe/p1273.waiver ${SBR_DIR}/cfg/tsa/
   cp ${CONST_PARSEXML}/febe/p1274.waiver ${SBR_DIR}/cfg/tsa/
   cp ${CONST_PARSEXML}/febe/p1222.waiver ${SBR_DIR}/cfg/tsa/
   cp ${CONST_PARSEXML}/febe/p1222.waiver ${SBR_DIR}/cfg/tsa/LKF1B.waiver
   cat ${POST_PARSEXML}/${component}_LocalToolData_p1273_xml.pm ${COPY_FROM_DIR}/hdkcfg_p1273.txt ${CONST_PARSEXML}/febe/LocalToolData_cust_eof.pm > ${SBR_DIR}/cfg/LocalToolData_p1273.pm
   cat ${POST_PARSEXML}/${component}_LocalToolData_p1274_xml.pm ${COPY_FROM_DIR}/hdkcfg_p1274.txt ${CONST_PARSEXML}/febe/LocalToolData_cust_eof.pm > ${SBR_DIR}/cfg/LocalToolData_p1274.pm
   cat ${POST_PARSEXML}/${component}_LocalToolData_s14nm_xml.pm ${COPY_FROM_DIR}/hdkcfg_s14nm.txt ${CONST_PARSEXML}/febe/LocalToolData_cust_eof.pm > ${SBR_DIR}/cfg/LocalToolData_s14nm.pm
   cat ${POST_PARSEXML}/${component}_LocalToolData_p1222_xml.pm ${COPY_FROM_DIR}/hdkcfg_p1222.txt ${CONST_PARSEXML}/febe/LocalToolData_cust_eof.pm > ${SBR_DIR}/cfg/LocalToolData_p1222.pm
   cat ${POST_PARSEXML}/${component}_LocalToolData_n7_xml.pm ${COPY_FROM_DIR}/hdkcfg_n7.txt ${CONST_PARSEXML}/febe/LocalToolData_cust_eof.pm > ${SBR_DIR}/cfg/LocalToolData_n7.pm
   cat ${POST_PARSEXML}/${component}_LocalToolData_n6_xml.pm ${COPY_FROM_DIR}/hdkcfg_n6.txt ${CONST_PARSEXML}/febe/LocalToolData_cust_eof.pm > ${SBR_DIR}/cfg/LocalToolData_n6.pm
   cp ${SBR_DIR}/cfg/LocalToolData_p1222.pm ${SBR_DIR}/cfg/LocalToolData_LKF1B.pm
   cp ${CONST_PARSEXML}/febe/ToolData.pm ${SBR_DIR}/cfg/ToolData.pm
   cp ${CONST_PARSEXML}/febe/custom_post.env.template ${SBR_DIR}/cfg/ace/templates/custom_post.env.template

   set FEBE_DIR=${TOOLS_DIR}/febe/inputs
   mkdir -p ${FEBE_DIR}
   cp ${CONST_PARSEXML}/febe/febe_populate.yaml ${FEBE_DIR}
   cp ${CONST_PARSEXML}/febe/febe_override_dc_config_tsa_prime.pm ${FEBE_DIR}/override_dc_config.pm
   #cp ${CONST_PARSEXML}/febe/febe_override_dc_config.pm ${FEBE_DIR}/override_dc_config.pm
   #cp ${CONST_PARSEXML}/febe/febe_override_dc.ot ${FEBE_DIR}/override_dc.ot


   ######## FEBE UPF ########
   mv ${TOOLS_DIR}/upf/router/${config}_${component}_sbx_top.upf      ${TOOLS_DIR}/upf/router/${RTLTOP}_wrapper.upf

   ######## FEBE UPF ########
   set UPF_FEBE_DIR=${TOOLS_DIR}/upf/inputs
   mkdir -p ${UPF_FEBE_DIR}
   cp ${TOOLS_DIR}/upf/router/${config}_${component}_top.upf          ${UPF_FEBE_DIR}/${RTLTOP}.upf

   set UPF2p0_FEBE_DIR=${TOOLS_DIR}/upf/router_upf2p0
#  mkdir -p ${UPF2p0_FEBE_DIR}
#  cp ${TOOLS_DIR}/upf/router/${config}_${component}_UPF2p0.upf       ${UPF2p0_FEBE_DIR}/${config}_${component}.upf
#  cp ${TOOLS_DIR}/upf/router/${config}_${component}_UPF2p0.upf       ${UPF2p0_FEBE_DIR}/${RTLTOP}_cth.upf
#  cp ${TOOLS_DIR}/upf/router/${config}_${component}.upf              ${UPF2p0_FEBE_DIR}/${config}_${component}_UPF1p0.upf
#  cp ${TOOLS_DIR}/upf/router/${config}_${component}_clamp_upf.cfg    ${UPF2p0_FEBE_DIR}
#  cp ${TOOLS_DIR}/upf/router/${config}_${component}_top.upf          ${UPF2p0_FEBE_DIR}
#  cp ${TOOLS_DIR}/upf/router/${config}_${component}_top_cth.upf      ${UPF2p0_FEBE_DIR}/${RTLTOP}_top_cth.upf
#  cp ${TOOLS_DIR}/upf/router/${RTLTOP}_wrapper.upf                   ${UPF2p0_FEBE_DIR}
#  cp ${TOOLS_DIR}/upf/router/${config}_top_UPF2p0.upf                ${UPF2p0_FEBE_DIR}/${config}_top.upf
#  cp ${TOOLS_DIR}/upf/router/${config}_top.upf                       ${UPF2p0_FEBE_DIR}/${config}_top_UPF1p0.upf
#  cp ${TOOLS_DIR}/upf/router/lpa_bind.tcl                            ${UPF2p0_FEBE_DIR}

   cp -r ${TOOLS_DIR}/upf/router ${UPF2p0_FEBE_DIR}
   mv ${UPF2p0_FEBE_DIR}/${config}_${component}.upf        ${UPF2p0_FEBE_DIR}/${config}_${component}_UPF1p0.upf
   cp ${UPF2p0_FEBE_DIR}/${config}_${component}_UPF2p0.upf ${UPF2p0_FEBE_DIR}/${config}_${component}.upf
   cp ${UPF2p0_FEBE_DIR}/${config}_${component}_UPF2p0.upf ${UPF2p0_FEBE_DIR}/${RTLTOP}_cth.upf
   mv ${UPF2p0_FEBE_DIR}/${config}_${component}_top_cth.upf ${UPF2p0_FEBE_DIR}/${RTLTOP}_top_cth.upf
   mv ${UPF2p0_FEBE_DIR}/${config}_top.upf                  ${UPF2p0_FEBE_DIR}/${config}_top_UPF1p0.upf
   cp ${UPF2p0_FEBE_DIR}/${config}_top_UPF2p0.upf           ${UPF2p0_FEBE_DIR}/${config}_top.upf


   ######## FEBE Caliber ########
   set CALIBER_FEBE_DIR=${TOOLS_DIR}/caliber/scripts
   mkdir -p ${CALIBER_FEBE_DIR}
   touch ${CALIBER_FEBE_DIR}/block_setup.tcl
   cp -r ${CONST_PARSEXML}/febe/caliber_waivers ${TOOLS_DIR}/caliber/waivers

   set CALIBER_FEBE_DIR=${TOOLS_DIR}/caliber/p1273/${RTLTOP}/scripts
   mkdir -p ${CALIBER_FEBE_DIR}
   set COPY_FROM_DIR=${POST_PARSEXML}
   cp  ${COPY_FROM_DIR}/${component}_block_setup_febe_caliber_p1273.tcl   ${CALIBER_FEBE_DIR}/block_setup.tcl
   cp  ${COPY_FROM_DIR}/${component}_clocks_caliber.stcl            ${CALIBER_FEBE_DIR}/${RTLTOP}_clocks.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/sbr_internal_exceptions.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/${RTLTOP}_constraints.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/bug.dummy

   set CALIBER_FEBE_DIR=${TOOLS_DIR}/caliber/p1274/${RTLTOP}/scripts
   mkdir -p ${CALIBER_FEBE_DIR}
   set COPY_FROM_DIR=${POST_PARSEXML}
   cp  ${COPY_FROM_DIR}/${component}_block_setup_febe_caliber_p1274.tcl   ${CALIBER_FEBE_DIR}/block_setup.tcl
   cp  ${COPY_FROM_DIR}/${component}_clocks_caliber.stcl            ${CALIBER_FEBE_DIR}/${RTLTOP}_clocks.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/sbr_internal_exceptions.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/${RTLTOP}_constraints.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/bug.dummy

   set CALIBER_FEBE_DIR=${TOOLS_DIR}/caliber/n7/${RTLTOP}/scripts
   mkdir -p ${CALIBER_FEBE_DIR}
   set CALIBER_FEBE_DIR_N6=${TOOLS_DIR}/caliber/n6/${RTLTOP}/scripts
   mkdir -p ${CALIBER_FEBE_DIR_N6}
   set COPY_FROM_DIR=${POST_PARSEXML}
   cp  ${COPY_FROM_DIR}/${component}_block_setup_febe_caliber_p1274.tcl   ${CALIBER_FEBE_DIR}/block_setup.tcl
   cp  ${COPY_FROM_DIR}/${component}_block_setup_febe_caliber_p1274.tcl   ${CALIBER_FEBE_DIR_N6}/block_setup.tcl
   cp  ${COPY_FROM_DIR}/${component}_clocks_caliber.stcl            ${CALIBER_FEBE_DIR}/${RTLTOP}_clocks.tcl
   cp  ${COPY_FROM_DIR}/${component}_clocks_caliber.stcl            ${CALIBER_FEBE_DIR_N6}/${RTLTOP}_clocks.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/sbr_internal_exceptions.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR_N6}/sbr_internal_exceptions.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/${RTLTOP}_constraints.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR_N6}/${RTLTOP}_constraints.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/bug.dummy
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR_N6}/bug.dummy

   set CALIBER_FEBE_DIR=${TOOLS_DIR}/caliber/s14nm/${RTLTOP}/scripts
   mkdir -p ${CALIBER_FEBE_DIR}
   set COPY_FROM_DIR=${POST_PARSEXML}
   cp  ${COPY_FROM_DIR}/${component}_block_setup_febe_caliber_s14nm.tcl   ${CALIBER_FEBE_DIR}/block_setup.tcl
   cp  ${COPY_FROM_DIR}/${component}_clocks_caliber.stcl            ${CALIBER_FEBE_DIR}/${RTLTOP}_clocks.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/sbr_internal_exceptions.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/${RTLTOP}_constraints.tcl
   cp  ${CONST_PARSEXML}/febe/caliber_bug.dummy                     ${CALIBER_FEBE_DIR}/bug.dummy

   ######## Openlatch ########
   cp -r ${CONST_PARSEXML}/openlatch ${TOOLS_DIR}
   set OPENLATCH_DIR=${SBR_DIR}/verif/tests/static_checks/openlatch
   mkdir -p ${OPENLATCH_DIR}
   set OPENLATCH_DIR=${SBR_DIR}/verif/tests/static_checks/openlatch/${RTLTOP}
   mkdir -p ${OPENLATCH_DIR}
   cp ${POST_PARSEXML}/${component}_openlatch.opt   ${OPENLATCH_DIR}/${RTLTOP}.opt

   ######## n7 Cheetah FLG ########
   #set N7_FLG_DUMP_HIER_DIR=${SBR_DIR}/verif/tests/dump_hier_test
   set N7_FLG_DUMP_HIER_DIR_STATIC=${SBR_DIR}/verif/tests/static_checks/dump_hier_test
   mkdir -p ${N7_FLG_DUMP_HIER_DIR_STATIC}
   #cp  ${CONST_PARSEXML}/dump_hier_test.sv ${N7_FLG_DUMP_HIER_DIR}
   #cp  ${CONST_PARSEXML}/dump_hier_test.opt ${N7_FLG_DUMP_HIER_DIR}
   #cp  ${CONST_PARSEXML}/dump_hier_postsim.pp ${N7_FLG_DUMP_HIER_DIR}
   cp  ${CONST_PARSEXML}/febe/dump_hier_test.sv ${N7_FLG_DUMP_HIER_DIR_STATIC}
   cp  ${CONST_PARSEXML}/febe/dump_hier_test.opt ${N7_FLG_DUMP_HIER_DIR_STATIC}
   cp  ${CONST_PARSEXML}/febe/dump_hier_postsim.pp ${N7_FLG_DUMP_HIER_DIR_STATIC}

   ######## FEBE FEV 1273 ########
   set FEV_FEBE_DIR=${TOOLS_DIR}/lec/inputs/p1273/${RTLTOP}/scripts
   mkdir -p ${FEV_FEBE_DIR}

   cp ${CONST_PARSEXML}/febe/block_setup_lec.tcl ${FEV_FEBE_DIR}/block_setup.tcl
   cp ${CONST_PARSEXML}/febe/hier_fev_prepend_lec.do ${FEV_FEBE_DIR}/hier_fev_prepend.do
   cp ${CONST_PARSEXML}/febe/hier_fev_append_lec.do ${FEV_FEBE_DIR}/hier_fev_append.do
   cp ${CONST_PARSEXML}/febe/pin_constraints_lec.tcl ${FEV_FEBE_DIR}/pin_constraints.tcl
   cp ${CONST_PARSEXML}/febe/PRE_read_upf_lec_p1273.tcl ${FEV_FEBE_DIR}/PRE.read_upf.tcl
   cp ${CONST_PARSEXML}/febe/PRE_map_lec_p1273.tcl ${FEV_FEBE_DIR}/PRE.map.tcl

   ######## FEBE FEV 1274 ########
   set FEV_FEBE_DIR=${TOOLS_DIR}/lec/inputs/p1274/${RTLTOP}/scripts
   mkdir -p ${FEV_FEBE_DIR}

   cp ${CONST_PARSEXML}/febe/block_setup_lec.tcl ${FEV_FEBE_DIR}/block_setup.tcl
   cp ${CONST_PARSEXML}/febe/hier_fev_prepend_lec.do ${FEV_FEBE_DIR}/hier_fev_prepend.do
   cp ${CONST_PARSEXML}/febe/hier_fev_append_lec.do ${FEV_FEBE_DIR}/hier_fev_append.do
   cp ${CONST_PARSEXML}/febe/pin_constraints_lec.tcl ${FEV_FEBE_DIR}/pin_constraints.tcl

   ######## FEBE FEV n7 ########
   set FEV_FEBE_DIR=${TOOLS_DIR}/lec/inputs/n7/${RTLTOP}/scripts
   mkdir -p ${FEV_FEBE_DIR}
   set FEV_FEBE_DIR_N6=${TOOLS_DIR}/lec/inputs/n6/${RTLTOP}/scripts
   mkdir -p ${FEV_FEBE_DIR_N6}

   cp ${CONST_PARSEXML}/febe/block_setup_lec.tcl ${FEV_FEBE_DIR}/block_setup.tcl
   cp ${CONST_PARSEXML}/febe/block_setup_lec.tcl ${FEV_FEBE_DIR_N6}/block_setup.tcl
   cp ${CONST_PARSEXML}/febe/hier_fev_prepend_lec.do ${FEV_FEBE_DIR}/hier_fev_prepend.do
   cp ${CONST_PARSEXML}/febe/hier_fev_prepend_lec.do ${FEV_FEBE_DIR_N6}/hier_fev_prepend.do
   cp ${CONST_PARSEXML}/febe/hier_fev_append_lec.do ${FEV_FEBE_DIR}/hier_fev_append.do
