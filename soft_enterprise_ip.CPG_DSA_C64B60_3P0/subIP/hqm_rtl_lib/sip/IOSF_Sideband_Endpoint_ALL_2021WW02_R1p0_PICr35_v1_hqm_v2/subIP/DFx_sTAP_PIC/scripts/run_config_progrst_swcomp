ace -cc -pwa pwa_progrst_swcomp
ace -x TapTestOnlyBypass \
       TapTestOnlySLVIDCODE \
       TapTestProgRstITDR \
       TapTestProgRstRTDR -pwa pwa_progrst_swcomp -nb -nbmon

grep -ir status pwa/results/tests/*/postsim.log

