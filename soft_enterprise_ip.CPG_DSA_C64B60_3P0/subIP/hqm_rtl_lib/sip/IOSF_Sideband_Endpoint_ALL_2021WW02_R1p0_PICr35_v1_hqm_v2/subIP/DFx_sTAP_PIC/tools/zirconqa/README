########################## ZIRCONQA 2.10 ###################################
# Command line
/p/hdk/rtl/proj_tools/zircon/master/2.10.02.P1/bin/zirconQA -model_root ./ -ipconfigid 88192 -dssmsid 99944 -alias stap -output tools/zirconqa/outputs/ -ovf tools/zirconqa/inputs/zirconqa_overrides_2.10.ini

######################### Switches that can be used ########################
 -gui                To open GUI
 -model_root         Path to RTL repo root
 -alias              Aliases. Support multiple: e.g. -alias sippcie_top sippcie
 -ovf                Change in override file format
 -fn /-rule          Enhanced to support list. e.g. -rule 11.105.a  12.124.a  3.50.a
 -cat                Enhanced to support list. e.g.   -cat CDC  Power 'Phys Design Enabling¿
 -priority	         Filter rules by priority
 -dumpdefaultovf     Generates override file that contains all keys and their associated default values
 -runall	            Run all rules regardless of applicability and milestone
 -norun	            Don¿t run the rules, lists which rules would be run
 -nodss	            Ignore ipconfig and dssmsid
 -verbose            Display details when the rules are run
#############################################################################
 DSS:               -ipconfigid, -ipconfig, -dssmsid, -subipconfig, -nodss
 Rule selection:    -auto, -manual, -category, -fn/-rule, -norun, -nonpor, -app, -ms, -priority, -runall
 Upload:            -ipDashUpload, -extupload, -release (WIP)
 Model:             -model_root, -alias, -dut, -ovf
 Listing/help:      -list, -listauto, -listmanual, -listipconfig, -listsubipconfig, -listdssms, -listkeys, -listkeysrules, -listruleskeys, -listalgo, -listms, -listapp, -listcat, -dumpdefaultovf, -man, -showcol (WIP)
 Others:            -optfile,  -output, -verbose

############################################################################

############### Use of cat switch ##########################################

/p/hdk/rtl/proj_tools/zircon/master/2.10.02.P1/bin/zirconQA   -dssms 74911 -subipconfig   LKFC-PSF0 -alias lkfc-psf0 psf0 -output tools/zirconqa/outputs -ovf  tools/zirconqa/2.10/ovf.ini -cat CDC Power 'Build & run' -priority High  -verbose 

############## TO listkeys #################################################

/p/hdk/rtl/proj_tools/zircon/master/2.10.02.P1/bin/zirconQA  -listkeys

############## To List dssms ###############################################
/p/hdk/rtl/proj_tools/zircon/master/2.10.02.P1/bin/zirconQA  -listdssms

############## To Run zircon without ipconfig or dssms ###############################################
/p/hdk/rtl/proj_tools/zircon/master/2.10.02.P1/bin/zirconQA -nodss
