   \cp tools/cdcLint/TGPLP/stap_waiver_w_TGPLP.xml tools/cdcLint/stap_waiver_w.xml
   setenv MODEL_ROOT $cwd
   setenv IP_ROOT $cwd
   setenv CDCLINT_HOME /p/hdk/rtl/proj_tools/cdclint/master/15.42.0/
   /p/hdk/rtl/proj_tools/cdclint/master/15.42.0/run_cdc_lint -res_dir tools/cdc/TGPLP/reports/
   
   \rm tools/cdcLint/TGPLP/stap_violations.xml 
   \rm tools/cdcLint/TGPLP/stap_violations.out 

   \cp tools/cdcLint/stap_violations.xml tools/cdcLint/TGPLP/
   \cp tools/cdcLint/stap_violations.out tools/cdcLint/TGPLP/

   \rm  tools/cdcLint/stap_violations.xml
