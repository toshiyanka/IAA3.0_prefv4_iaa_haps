               pli_libs => [qw(-debussy_pli_libs)],
               export => {
                  libs => [qw(
                     jtag_bfm_ti_lib
                     jtag_bfm_pkg_lib
                  )],
               },
            },
            jtagbfm_UVM_model => {
               top => 'tapnw_UVM_tb_lib.top',
               libs => [qw(
                  tapnw_UVM_test_lib
                  tapnw_UVM_tb_lib
                  tapnw_UVM_ti_lib
                  tapnw_UVM_pkg_lib
                  jtag_bfm_UVM_ti_lib
                  jtag_bfm_UVM_pkg_lib
                  tapnw_rtl_lib
                  stap_rtl_lib
                  mtap_rtl_lib
                  sip_shared_lib
               )],
               pli_libs => [qw(-debussy_pli_libs)],
               export => {
                  libs => [qw(
                     jtag_bfm_UVM_ti_lib
                     jtag_bfm_UVM_pkg_lib
                  )],
               },
            },
            converged_jtagbfm_model => {
               top => 'tapnw_tb_lib_converged.top',
               libs => [qw(
                  tapnw_test_lib_converged
                  tapnw_tb_lib_converged
                  tapnw_ti_lib_converged
                  tapnw_pkg_lib_converged
                  converged_jtag_bfm_pkg_lib
                  tapnw_rtl_lib
                  stap_rtl_lib
                  mtap_rtl_lib
                  sip_shared_lib
               )],
               pli_libs => [qw(-debussy_pli_libs)],
               export => {
                  libs => [qw(
                     converged_jtag_bfm_pkg_lib
                  )],
               },
            },
         }, #end models
      },
   }, # end of HDLSpec
);
