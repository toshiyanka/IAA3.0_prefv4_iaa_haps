            },
            tapnw_test_lib => {
               -hdl_spec => [ "subIP/ip-tap-network/verif/tests/tests.hdl", ],
               -vlog_incdirs => [ "$ENV{OVM_HOME}/src/", "$ENV{UVM_HOME}/src", "$ENV{XVM_HOME}/src/", "$ENV{XVM_HOME}/src/macros", ],
               -dependent_libs => [ "sip_shared_lib", "tapnw_tb_lib", "tapnw_pkg_lib", "jtag_bfm_pkg_lib", ],
            },
            tapnw_UVM_test_lib => {
               -hdl_spec => [ "subIP/ip-tap-network/verif_UVM/tests/tests.hdl", ],
               -vlog_incdirs => [ "$ENV{UVM_HOME}/src/", "$ENV{OVM_HOME}/src", "$ENV{XVM_HOME}/src/", "$ENV{XVM_HOME}/src/macros", ],
               -dependent_libs => [ "sip_shared_lib", "tapnw_UVM_tb_lib", "tapnw_UVM_pkg_lib", "jtag_bfm_UVM_pkg_lib", ],
            },
            tapnw_test_lib_converged => {
               -hdl_spec => [ "subIP/ip-tap-network/verif/tests/tests.hdl", ],
               -vlog_incdirs => [ "$ENV{OVM_HOME}/src/", "$ENV{UVM_HOME}/src", "$ENV{XVM_HOME}/src/", "$ENV{XVM_HOME}/src/macros", ],
               -dependent_libs => [ "sip_shared_lib", "tapnw_tb_lib_converged", "tapnw_pkg_lib_converged", "converged_jtag_bfm_pkg_lib", ],
            },
         },
         models => {
            jtagbfm_model => {
               top => 'tapnw_tb_lib.top',
               libs => [qw(
                  tapnw_test_lib
                  tapnw_tb_lib
                  tapnw_ti_lib
                  tapnw_pkg_lib
                  jtag_bfm_ti_lib
                  jtag_bfm_pkg_lib
