               -dependent_libs => [ "sip_shared_lib", "jtag_bfm_pkg_lib",],
            },
            tapnw_UVM_pkg_lib => {
               -hdl_spec => [ "subIP/ip-tap-network/verif_UVM/tb/tapnw_pkg.hdl", ],
               -vlog_incdirs => [ "$ENV{UVM_HOME}/src/", "$ENV{OVM_HOME}/src", "$ENV{XVM_HOME}/src/", "$ENV{XVM_HOME}/src/macros", ],
               -dependent_libs => [ "sip_shared_lib", "jtag_bfm_UVM_pkg_lib",],
