            },
            tapnw_UVM_tb_lib => {
               -hdl_spec => [ "subIP/ip-tap-network/verif_UVM/tb/tb.hdl", ],
               -vlog_incdirs => [ "$ENV{UVM_HOME}/src/", "$ENV{OVM_HOME}/src", ],
               -dependent_libs => [ "sip_shared_lib", "tapnw_UVM_pkg_lib", "jtag_bfm_UVM_pkg_lib", "tapnw_rtl_lib", "tapnw_UVM_ti_lib", "stap_rtl_lib", "mtap_rtl_lib", "jtag_bfm_UVM_ti_lib", ],
            },
