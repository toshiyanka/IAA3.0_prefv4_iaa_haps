##19-Jan-13 Renamed CTT_GENERATED_FILES to GENERATED_FILES_FOR_JTAGBFM

my $PROJECT = "jtagbfm";
%audf = (
   HDLSpec => {
      -hdl_compiler => {
         vcs => {
            -iniContents => <<'EOF',
            --Simulation variables
            TIMEBASE        = PS
            TIME_RESOLUTION = 1ps

            --Mapping default work dir
            work > jtagbfm_vcs_testlib
            --Mapping system libraries
EOF
         },
      },
      $PROJECT => {
         libs => {
	    uvm_pkg_lib               => { 
               -tag                   =>   'nonsynth',
               -vlog_files            => [ "$ENV{UVM_HOME}/src/uvm_pkg.sv", ],
               -vlog_incdirs          => [ "$ENV{UVM_HOME}/src", ], 
               -vlog_opts             => [ "-sverilog", ],
            },
	    
	    ovm_pkg_lib               => { 
               -tag                   =>   'nonsynth',
               -vlog_files            => [ "$ENV{OVM_HOME}/src/ovm_pkg.sv", ],
               -vlog_incdirs          => [ "$ENV{OVM_HOME}/src", ], 
               -vlog_opts             => [ "-sverilog", ],
            },
	    
	    xvm_pkg_lib               => { 
