# IOSF signasl
#-------------------------------------------------------------------------------

set sbep_isolation_clamp0_iosf ""
lappend sbep_isolation_clamp0_iosf "${path_to_pg_domain}side_ism_agent\[*\]"

lappend sbep_isolation_clamp0_iosf "${path_to_pg_domain}mpcput"
lappend sbep_isolation_clamp0_iosf "${path_to_pg_domain}mnpput"
lappend sbep_isolation_clamp0_iosf "${path_to_pg_domain}meom"
lappend sbep_isolation_clamp0_iosf "${path_to_pg_domain}mpayload\[*\]"

lappend sbep_isolation_clamp0_iosf "${path_to_pg_domain}tpccup"
lappend sbep_isolation_clamp0_iosf "${path_to_pg_domain}tnpcup"



set sbep_isolation_clamp1_iosf "${path_to_pg_domain}side_clkreq"






#-------------------------------------------------------------------------------
# Apply the 0-clamps to the list of signals
#-------------------------------------------------------------------------------


set  sbep_isolation_clamp0 ""
set  sbep_isolation_clamp0  [concat $sbep_isolation_clamp0 $sbep_isolation_clamp0_iosf]
set  sbep_isolation_clamp0  [concat $sbep_isolation_clamp0 $sbep_isolation_clamp0_agent_misc]
set  sbep_isolation_clamp0  [concat $sbep_isolation_clamp0 $sbep_isolation_clamp0_agent_treg]
set  sbep_isolation_clamp0  [concat $sbep_isolation_clamp0 $sbep_isolation_clamp0_agent_mreg]
set  sbep_isolation_clamp0  [concat $sbep_isolation_clamp0 $sbep_isolation_clamp0_agent_tmsg]
set  sbep_isolation_clamp0  [concat $sbep_isolation_clamp0 $sbep_isolation_clamp0_agent_mmsg]
set  sbep_isolation_clamp0  [concat $sbep_isolation_clamp0 $sbep_isolation_clamp0_dfx]

set_isolation              io_clamp0_sbep_top              \
    -domain                $sbep_power_domain              \
    -elements              $sbep_isolation_clamp0          \
    -isolation_power_net   ${SOC_SUPPLY_PORT}              \
    -isolation_ground_net  ${SOC_GROUND_PORT}              \
    -clamp_value           0
 
set_isolation_control      io_clamp0_sbep_top              \
     -domain               $sbep_power_domain              \
     -isolation_signal     $sbep_isol_signal               \
     -isolation_sense      low                             \
     -location             ${SOC_ISOLATION_CELL_LOCATION}


map_isolation_cell          io_clamp0_sbep_top  \
     -domain                $sbep_power_domain \
     -lib_cells             $FW_AND_CELL  



#-------------------------------------------------------------------------------
# Apply the 1-clamps to the list of signals
#-------------------------------------------------------------------------------
set  sbep_isolation_clamp1 ""
set  sbep_isolation_clamp1 [concat $sbep_isolation_clamp1 $sbep_isolation_clamp1_agent_misc]
set  sbep_isolation_clamp1 [concat $sbep_isolation_clamp1 $sbep_isolation_clamp1_agent_mmsg]
set  sbep_isolation_clamp1 [concat $sbep_isolation_clamp1 $sbep_isolation_clamp1_iosf]

set_isolation              io_clamp1_sbep_top              \
    -domain                $sbep_power_domain              \
    -elements              $sbep_isolation_clamp1          \
    -isolation_power_net   ${SOC_SUPPLY_PORT}              \
    -isolation_ground_net  ${SOC_GROUND_PORT}              \
    -clamp_value           1
 
set_isolation_control      io_clamp1_sbep_top              \
     -domain               $sbep_power_domain              \
     -isolation_signal     $sbep_isol_signal               \
     -isolation_sense      low                             \
     -location             ${SOC_ISOLATION_CELL_LOCATION}

map_isolation_cell          io_clamp1_sbep_top  \
     -domain               $sbep_power_domain \
     -lib_cells            $FW_OR_CELL  


